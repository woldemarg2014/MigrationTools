					НоваяСтрока.Сумма     = НоваяСтрока.СуммаВсего;
				Иначе
					НоваяСтрока.СтавкаНДС = Справочники.СтавкиНДС.ОсновнаяСтавкаНДС; //gavv пока так
					НоваяСтрока.Сумма = НоваяСтрока.СуммаВсего;
				КонецЕсли;
				
				// Пересчитываем цену.
				Если НоваяСтрока.Количество &lt;&gt; 0 Тогда
					НоваяСтрока.Цена = Окр(НоваяСтрока.Сумма / НоваяСтрока.Количество, 2);
				КонецЕсли;
				
		КонецЦикла;
		
		Если ТаблицаДанных.Количество() &lt;&gt; 0 Тогда
			
			ИтогСумма    = ТаблицаДанных.Итог("СуммаВсего");
			ИтогСуммаУпр = ТаблицаДанных.Итог("СуммаУпр");
			
			Если ИтогСуммаУпр = 0 Тогда
				КурсВалютыУпрДокумента = КурсВалютыУпр;
			Иначе
				КурсВалютыУпрДокумента = ИтогСумма / ИтогСуммаУпр;
			КонецЕсли;
			
			ВходящиеДанные	= Новый Структура("Дата, ВалютаДокумента, ДоговорВзаиморасчетов, Комментарий, Контрагент, КурсВалютыУпр, КурсДокумента, Организация, ПодразделениеКомпании, СкладКомпании, ХозОперация, Автомобили");
			ВходящиеДанные.Дата						= НаДату;
			ВходящиеДанные.ВалютаДокумента			= ВалютаРегл;
			ВходящиеДанные.ДоговорВзаиморасчетов	= ТекДоговорВзаиморасчетов;
			ВходящиеДанные.Контрагент				= ТекКонтрагент;
			ВходящиеДанные.КурсВалютыУпр			= КурсВалютыУпрДокумента;
			ВходящиеДанные.КурсДокумента			= КурсДокумента;
			ВходящиеДанные.Организация				= ТекОрганизация;
			ВходящиеДанные.ПодразделениеКомпании	= ТекПодразделение;
			ВходящиеДанные.СкладКомпании			= ТекСкладКомпании;
			ВходящиеДанные.ХозОперация				= ХозОперацияОстатки;
			ВходящиеДанные.Комментарий				= "Перенос остатков принятых на комиссию автомобилей из Альфа-Авто 5. Партия-источник: "+Строка(ТекПартия);
			ВходящиеДанные.Автомобили				= ТаблицаДанных;
			
			//***
			ВыгрузитьПоПравилу(,, ВходящиеДанные,, "ОстаткиАвтомобилей");
			ТаблицаДанных.Очистить();
		КонецЕсли;
		
	КонецЦикла;
КонецЦикла;</ПередОбработкойПравила>
			</Правило>
			<Правило Отключить="false">
				<Код>Остатки_ПереданныхНаКомиссиюАвтомобилей</Код>
