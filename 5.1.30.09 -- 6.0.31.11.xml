// Выполняем свертку партий
ТаблицаПоложительныхОстатков = РезультатЗапроса[1].Выгрузить();
ТаблицаПоложительныхОстатков.Индексы.Добавить("СкладКомпании,Номенклатура,ХарактеристикаНоменклатуры,СтатусПартии");

ТаблицаОтрицательныхОстатков = РезультатЗапроса[2].Выгрузить();

СтруктураПоиска = Новый Структура("СкладКомпании,Номенклатура,ХарактеристикаНоменклатуры,СтатусПартии");
Для Каждого СтрокаОтрицательныхОстатков Из ТаблицаОтрицательныхОстатков Цикл
	
	ЗаполнитьЗначенияСвойств(СтруктураПоиска, СтрокаОтрицательныхОстатков);
	Для Каждого НайденнаяСтрока Из ТаблицаПоложительныхОстатков.НайтиСтроки(СтруктураПоиска) Цикл
		
		Если СтрокаОтрицательныхОстатков.КоличествоОстаток = 0 Тогда
			Прервать;
		КонецЕсли;
		
		Если НайденнаяСтрока.КоличествоОстаток = 0 Тогда
			Продолжить;
		КонецЕсли;
		
		КоличествоКСписанию = Мин(СтрокаОтрицательныхОстатков.КоличествоОстаток, НайденнаяСтрока.КоличествоОстаток);
		Если КоличествоКСписанию = НайденнаяСтрока.КоличествоОстаток Тогда
			НайденнаяСтрока.КоличествоОстаток = 0;
			НайденнаяСтрока.СуммаОстаток      = 0;
			НайденнаяСтрока.СуммаНДСОстаток   = 0;
			НайденнаяСтрока.СуммаУпрОстаток   = 0;
		Иначе
			
			КоличествоСтроки = НайденнаяСтрока.КоличествоОстаток;
			НайденнаяСтрока.КоличествоОстаток = НайденнаяСтрока.КоличествоОстаток - КоличествоКСписанию;
			НайденнаяСтрока.СуммаОстаток      = НайденнаяСтрока.СуммаОстаток    / КоличествоСтроки * НайденнаяСтрока.КоличествоОстаток;
			НайденнаяСтрока.СуммаНДСОстаток   = НайденнаяСтрока.СуммаНДСОстаток / КоличествоСтроки * НайденнаяСтрока.КоличествоОстаток;
			НайденнаяСтрока.СуммаУпрОстаток   = НайденнаяСтрока.СуммаУпрОстаток / КоличествоСтроки * НайденнаяСтрока.КоличествоОстаток;
			
		КонецЕсли;
		
		СтрокаОтрицательныхОстатков.КоличествоОстаток = СтрокаОтрицательныхОстатков.КоличествоОстаток - КоличествоКСписанию;
		
	КонецЦикла;
	
КонецЦикла;

// Скорректируем таблицу партий по остаткам на складах.
// Если по партиям остаток больше, чем на складе, то остаток по партиям нужно уменьшить до остатка на складе.
// Если по партиям остатко меньше, чем на складе, то в остаток по партиям нужно добавить разницу. Разница добавляется в самую последнюю партию.
ЗапросОстатков = Новый Запрос;
ЗапросОстатков.Текст =
"ВЫБРАТЬ
|	ТаблицаОстатков.Партия КАК Партия,
|	ТаблицаОстатков.СтатусПартии,
|	ТаблицаОстатков.СкладКомпании КАК СкладКомпании,
|	ТаблицаОстатков.Номенклатура КАК Номенклатура,
|	ТаблицаОстатков.ХарактеристикаНоменклатуры КАК ХарактеристикаНоменклатуры,
|	ТаблицаОстатков.КоличествоОстаток КАК КоличествоОстаток,
|	ТаблицаОстатков.СуммаОстаток КАК СуммаОстаток,
|	ТаблицаОстатков.СуммаНДСОстаток КАК СуммаНДСОстаток,
|	ТаблицаОстатков.СуммаУпрОстаток КАК СуммаУпрОстаток
|ПОМЕСТИТЬ ВТ_ТаблицаОстатковПартий
|ИЗ
|	&amp;ТаблицаОстатков КАК ТаблицаОстатков
|;
|
|////////////////////////////////////////////////////////////////////////////////
|ВЫБРАТЬ
|	ВТ_ТаблицаОстатковПартий.Партия,
|	ВТ_ТаблицаОстатковПартий.СтатусПартии,
|	ВТ_ТаблицаОстатковПартий.СкладКомпании КАК СкладКомпании,
|	ВТ_ТаблицаОстатковПартий.Номенклатура КАК Номенклатура,
|	ВТ_ТаблицаОстатковПартий.ХарактеристикаНоменклатуры КАК ХарактеристикаНоменклатуры,
|	ВТ_ТаблицаОстатковПартий.КоличествоОстаток,
|	ВТ_ТаблицаОстатковПартий.СуммаОстаток,
|	ВТ_ТаблицаОстатковПартий.СуммаНДСОстаток,
|	ВТ_ТаблицаОстатковПартий.СуммаУпрОстаток
|ПОМЕСТИТЬ ВТ_ТаблицаОстатков
|ИЗ
|	ВТ_ТаблицаОстатковПартий КАК ВТ_ТаблицаОстатковПартий
|ГДЕ
|	ВТ_ТаблицаОстатковПартий.КоличествоОстаток &gt; 0
|
|ИНДЕКСИРОВАТЬ ПО
|	СкладКомпании,
|	Номенклатура,
|	ХарактеристикаНоменклатуры
|;
|
|////////////////////////////////////////////////////////////////////////////////
|ВЫБРАТЬ
|	ОстаткиТоваровКомпанииОстатки.СкладКомпании КАК СкладКомпании,
|	ОстаткиТоваровКомпанииОстатки.Номенклатура КАК Номенклатура,
|	ОстаткиТоваровКомпанииОстатки.ХарактеристикаНоменклатуры КАК ХарактеристикаНоменклатуры,
|	ОстаткиТоваровКомпанииОстатки.КоличествоОстаток
|ПОМЕСТИТЬ ВТ_ОстаткиТоваров
|ИЗ
|	РегистрНакопления.ОстаткиТоваровКомпании.Остатки(&amp;ДатаСведений, ) КАК ОстаткиТоваровКомпанииОстатки
|ГДЕ
|	ОстаткиТоваровКомпанииОстатки.КоличествоОстаток &gt; 0
|
|ИНДЕКСИРОВАТЬ ПО
|	СкладКомпании,
|	Номенклатура,
|	ХарактеристикаНоменклатуры
|;
|
|////////////////////////////////////////////////////////////////////////////////
|ВЫБРАТЬ
|	ВТ_ОстаткиТоваров.СкладКомпании КАК СкладКомпании,
|	ВТ_ОстаткиТоваров.Номенклатура КАК Номенклатура,
|	ВТ_ОстаткиТоваров.ХарактеристикаНоменклатуры КАК ХарактеристикаНоменклатуры,
|	ВТ_ОстаткиТоваров.КоличествоОстаток КАК ОстатокНаСкладе,
|	ЕСТЬNULL(ВТ_ТаблицаОстатков.Партия, НЕОПРЕДЕЛЕНО) КАК Партия,
|	ЕСТЬNULL(ВТ_ТаблицаОстатков.СтатусПартии, НЕОПРЕДЕЛЕНО) КАК СтатусПартии,
|	ЕСТЬNULL(ВТ_ТаблицаОстатков.КоличествоОстаток, 0) КАК КоличествоОстаток,
|	ЕСТЬNULL(ВТ_ТаблицаОстатков.СуммаОстаток, 0) КАК СуммаОстаток,
|	ЕСТЬNULL(ВТ_ТаблицаОстатков.СуммаНДСОстаток, 0) КАК СуммаНДСОстаток,
|	ЕСТЬNULL(ВТ_ТаблицаОстатков.СуммаУпрОстаток, 0) КАК СуммаУпрОстаток,
|	ВЫБОР
|		КОГДА ВТ_ТаблицаОстатков.СтатусПартии ЕСТЬ NULL
|			ТОГДА 2
|		КОГДА ВТ_ТаблицаОстатков.СтатусПартии = ЗНАЧЕНИЕ(Перечисление.СтатусыПартий.ТоварКупленный)
|			ТОГДА 0
|		ИНАЧЕ 1
|	КОНЕЦ КАК ПолеСортировки
|ИЗ
|	ВТ_ОстаткиТоваров КАК ВТ_ОстаткиТоваров
|		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ТаблицаОстатков КАК ВТ_ТаблицаОстатков
|		ПО ВТ_ОстаткиТоваров.СкладКомпании = ВТ_ТаблицаОстатков.СкладКомпании
|			И ВТ_ОстаткиТоваров.Номенклатура = ВТ_ТаблицаОстатков.Номенклатура
|			И ВТ_ОстаткиТоваров.ХарактеристикаНоменклатуры = ВТ_ТаблицаОстатков.ХарактеристикаНоменклатуры
|
|УПОРЯДОЧИТЬ ПО
|	ПолеСортировки,
|	ВТ_ТаблицаОстатков.Партия
|ИТОГИ
|	МАКСИМУМ(ОстатокНаСкладе),
|	СУММА(КоличествоОстаток),
|	СУММА(СуммаОстаток),
|	СУММА(СуммаНДСОстаток),
|	СУММА(СуммаУпрОстаток)
|ПО
|	СкладКомпании,
|	Номенклатура,
|	ХарактеристикаНоменклатуры";
ЗапросОстатков.УстановитьПараметр("ДатаСведений"   , КонецДня(НаДату));
ЗапросОстатков.УстановитьПараметр("ТаблицаОстатков", ТаблицаПоложительныхОстатков);

ЗапросОпределенияПоследнейПартии = Новый Запрос;
ЗапросОпределенияПоследнейПартии.Текст =
"ВЫБРАТЬ ПЕРВЫЕ 1
|	ПартииТоваровКомпанииОбороты.Партия,
|	ПартииТоваровКомпанииОбороты.СтатусПартии,
|	ВЫБОР
|		КОГДА ПартииТоваровКомпанииОбороты.СтатусПартии = ЗНАЧЕНИЕ(Перечисление.СтатусыПартий.ТоварКупленный)
|			ТОГДА 0
|		ИНАЧЕ 1
|	КОНЕЦ КАК ПолеСортировки,
|	ПартииТоваровКомпанииОбороты.КоличествоПриход,
|	ПартииТоваровКомпанииОбороты.СуммаПриход,
|	ПартииТоваровКомпанииОбороты.СуммаНДСПриход,
|	ПартииТоваровКомпанииОбороты.СуммаУпрПриход
|ИЗ
|	РегистрНакопления.ПартииТоваровКомпании.Обороты(
|			,
|			,
|			,
|			СкладКомпании = &amp;СкладКомпании
|				И Номенклатура = &amp;Номенклатура
|				И ХарактеристикаНоменклатуры = &amp;ХарактеристикаНоменклатуры) КАК ПартииТоваровКомпанииОбороты
|ГДЕ
|	ПартииТоваровКомпанииОбороты.КоличествоПриход &gt; 0
|
|УПОРЯДОЧИТЬ ПО
|	ПолеСортировки,
|	ПартииТоваровКомпанииОбороты.Партия.МоментВремени УБЫВ";

РезультатЗапроса = ЗапросОстатков.Выполнить();

ТаблицаПоложительныхОстатков.Очистить();

ВыборкаПоСкладам = РезультатЗапроса.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);

Пока ВыборкаПоСкладам.Следующий() Цикл
	
	ВыборкаПоНоменклатуре = ВыборкаПоСкладам.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	Пока ВыборкаПоНоменклатуре.Следующий() Цикл
		
		ВыборкаПоХарактеристикам = ВыборкаПоНоменклатуре.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
		Пока ВыборкаПоХарактеристикам.Следующий() Цикл
			
			ОстатокНаСкладе = ВыборкаПоХарактеристикам.ОстатокНаСкладе;
			ОстатокПартий   = ВыборкаПоХарактеристикам.КоличествоОстаток;
			
			// Если по партиям остаток больше, чем на складе, то остаток по партиям нужно уменьшить до остатка на складе.
			Если ОстатокПартий &gt; ОстатокНаСкладе Тогда
				
				КоличествоРазницы = ОстатокПартий - ОстатокНаСкладе;
				
			// Если по партиям остатко меньше, чем на складе, то в остаток по партиям нужно добавить разницу. Разница добавляется в самую последнюю партию.
			ИначеЕсли ОстатокПартий &lt; ОстатокНаСкладе Тогда
				
				КоличествоРазницы = ОстатокПартий - ОстатокНаСкладе;
				
			Иначе // Переносим как есть
				
				КоличествоРазницы = 0;
				
			КонецЕсли;
			
			СчетчикВыборкиПартий = 1;
			ВыборкаПартий = ВыборкаПоХарактеристикам.Выбрать();
			Пока ВыборкаПартий.Следующий() Цикл
				
				НоваяСтрока = ТаблицаПоложительныхОстатков.Добавить();
				НоваяСтрока.СкладКомпании              = ВыборкаПоСкладам.СкладКомпании;
				НоваяСтрока.Номенклатура               = ВыборкаПоНоменклатуре.Номенклатура;
				НоваяСтрока.ХарактеристикаНоменклатуры = ВыборкаПоХарактеристикам.ХарактеристикаНоменклатуры;
				
				Если КоличествоРазницы &gt; 0 Тогда
					
					СтатусПартии = ВыборкаПартий.СтатусПартии;
					Партия       = ВыборкаПартий.Партия;
					
					Остаток = Мин(КоличествоРазницы, ВыборкаПартий.КоличествоОстаток);
					КоличествоРазницы = КоличествоРазницы - Остаток;
					КоличествоОстаток = ВыборкаПартий.КоличествоОстаток - Остаток;
					
					Если КоличествоОстаток = 0 ИЛИ ВыборкаПартий.КоличествоОстаток = 0 Тогда
						
						СуммаОстаток    = 0;
						СуммаНДСОстаток = 0;
						СуммаУпрОстаток = 0;
						
					ИначеЕсли КоличествоОстаток = ВыборкаПартий.КоличествоОстаток Тогда
						
						СуммаОстаток    = ВыборкаПартий.СуммаОстаток;
						СуммаНДСОстаток = ВыборкаПартий.СуммаНДСОстаток;
						СуммаУпрОстаток = ВыборкаПартий.СуммаУпрОстаток;
						
					Иначе
						
						СуммаОстаток    = ВыборкаПартий.СуммаОстаток    / ВыборкаПартий.КоличествоОстаток * КоличествоОстаток;
						СуммаНДСОстаток = ВыборкаПартий.СуммаНДСОстаток / ВыборкаПартий.КоличествоОстаток * КоличествоОстаток;
						СуммаУпрОстаток = ВыборкаПартий.СуммаУпрОстаток / ВыборкаПартий.КоличествоОстаток * КоличествоОстаток;
						
					КонецЕсли;
					
				ИначеЕсли КоличествоРазницы &lt; 0 Тогда
					
					Если ЗначениеЗаполнено(ВыборкаПартий.Партия) Тогда
						
						СтатусПартии = ВыборкаПартий.СтатусПартии;
						Партия       = ВыборкаПартий.Партия;
						
						КоличествоОстаток = ВыборкаПартий.КоличествоОстаток;
						СуммаОстаток      = ВыборкаПартий.СуммаОстаток;
						СуммаНДСОстаток   = ВыборкаПартий.СуммаНДСОстаток;
						СуммаУпрОстаток   = ВыборкаПартий.СуммаУпрОстаток;
						
						Если СчетчикВыборкиПартий = ВыборкаПартий.Количество() Тогда
							
							КоличествоОстаток = КоличествоОстаток - КоличествоРазницы;
							СуммаОстаток      = ВыборкаПартий.СуммаОстаток    / ВыборкаПартий.КоличествоОстаток * КоличествоОстаток;
							СуммаНДСОстаток   = ВыборкаПартий.СуммаНДСОстаток / ВыборкаПартий.КоличествоОстаток * КоличествоОстаток;
							СуммаУпрОстаток   = ВыборкаПартий.СуммаУпрОстаток / ВыборкаПартий.КоличествоОстаток * КоличествоОстаток;
							
							КоличествоРазницы = 0;
							
						КонецЕсли;
						
					Иначе
						
						ЗапросОпределенияПоследнейПартии.УстановитьПараметр("СкладКомпании"             , НоваяСтрока.СкладКомпании);
						ЗапросОпределенияПоследнейПартии.УстановитьПараметр("Номенклатура"              , НоваяСтрока.Номенклатура);
						ЗапросОпределенияПоследнейПартии.УстановитьПараметр("ХарактеристикаНоменклатуры", НоваяСтрока.ХарактеристикаНоменклатуры);
						
						ВыборкаПоследнейПартии = ЗапросОпределенияПоследнейПартии.Выполнить().Выбрать();
						Если ВыборкаПоследнейПартии.Следующий() Тогда
							
							СтатусПартии      = ВыборкаПоследнейПартии.СтатусПартии;
							Партия            = ВыборкаПоследнейПартии.Партия;
							КоличествоОстаток = -КоличествоРазницы;
							СуммаОстаток      = ВыборкаПоследнейПартии.СуммаПриход    / ВыборкаПоследнейПартии.КоличествоПриход * КоличествоОстаток;
							СуммаНДСОстаток   = ВыборкаПоследнейПартии.СуммаНДСПриход / ВыборкаПоследнейПартии.КоличествоПриход * КоличествоОстаток;
							СуммаУпрОстаток   = ВыборкаПоследнейПартии.СуммаУпрПриход / ВыборкаПоследнейПартии.КоличествоПриход * КоличествоОстаток;
							
						Иначе
							
							СтатусПартии      = Перечисления.СтатусыПартий.ТоварКупленный;
							Партия            = ПартияТоваровОтрицательныхОстатков;
							КоличествоОстаток = -КоличествоРазницы;
							СуммаОстаток      = 0;
							СуммаНДСОстаток   = 0;
							СуммаУпрОстаток   = 0;
							
						КонецЕсли;
						
						КоличествоРазницы = 0;
						
					КонецЕсли;
					
				Иначе
					
					СтатусПартии      = ВыборкаПартий.СтатусПартии;
					Партия            = ВыборкаПартий.Партия;
					КоличествоОстаток = ВыборкаПартий.КоличествоОстаток;
					СуммаОстаток      = ВыборкаПартий.СуммаОстаток;
					СуммаНДСОстаток   = ВыборкаПартий.СуммаНДСОстаток;
					СуммаУпрОстаток   = ВыборкаПартий.СуммаУпрОстаток;
					
				КонецЕсли;
				
				НоваяСтрока.СтатусПартии      = СтатусПартии;
				НоваяСтрока.Партия            = Партия;
				НоваяСтрока.КоличествоОстаток = КоличествоОстаток;
