			И НЕ (ЗначениеЗаполнено(Объект.СтатьяДопРасходов)) Тогда
			Объект.СтатьяДопРасходов = Справочники.СтатьиДоходовИРасходов.ПрочиеДоходыИРасходы;
			
			СтрокаСообщения = СтрокаСообщения + ?(ПустаяСтрока(СтрокаСообщения),"","; ") + "заполнен реквизит 'СтатьяДопРасходов'";
		КонецЕсли;
		
		Если Объект.ВидНоменклатуры.Пустая() Тогда
			Если НЕ Объект.ТипНоменклатуры.Пустая() Тогда
				Объект.ВидНоменклатуры = Объект.ТипНоменклатуры.ВидНоменклатуры;
				СтрокаСообщения = СтрокаСообщения + ?(ПустаяСтрока(СтрокаСообщения),"","; ") + "заполнены реквизиты 'ВидНоменклатуры'";
			КонецЕсли; 
		КонецЕсли;
		
		Если Объект.ТипНоменклатуры.Пустая() Тогда
			Объект.ТипНоменклатуры = Справочники.ТипыНоменклатуры.Штучный;
			Объект.ВидНоменклатуры = Объект.ТипНоменклатуры.ВидНоменклатуры;
			СтрокаСообщения = СтрокаСообщения + ?(ПустаяСтрока(СтрокаСообщения),"","; ") + "заполнены реквизиты 'ТипНоменклатуры', 'ВидНоменклатуры'";
		КонецЕсли;
		
		Если Объект.БазоваяЕдиницаИзмерения.Пустая() Тогда
			Объект.БазоваяЕдиницаИзмерения = Объект.ТипНоменклатуры.ОсновнаяБазоваяЕдиницаИзмерения;
			Если Объект.БазоваяЕдиницаИзмерения.Пустая() Тогда
				Объект.БазоваяЕдиницаИзмерения = Справочники.КлассификаторЕдиницИзмерения.шт;
				СтрокаСообщения = СтрокаСообщения + ?(ПустаяСтрока(СтрокаСообщения),"","; ") + "заполнены реквизиты 'БазоваяЕдиницаИзмерения'";
			КонецЕсли;
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(Объект.ОсновнаяЕдиницаИзмерения) Тогда
			Если Объект.ТипНоменклатуры.ИспользованиеЕдиницИзмерения = 1 Тогда
				НоваяЕдиницаИзмерения = Справочники.Номенклатура.ПолучитьОсновнуюЕдиницуПоБазовой(Объект);
				Объект.ОсновнаяЕдиницаИзмерения = НоваяЕдиницаИзмерения.Ссылка;
				СтрокаСообщения = СтрокаСообщения + ?(ПустаяСтрока(СтрокаСообщения),"","; ") + "заполнены реквизиты 'ОсновнаяЕдиницаИзмерения'";
			Иначе
				Объект.ОбменДанными.Загрузка = Истина;
				СтрокаСообщения = СтрокаСообщения + ?(ПустаяСтрока(СтрокаСообщения),"","; ") + "записан в режиме обмена данными, так как не заполнена 'ОсновнаяЕдиницаИзмерения' при подчинении 'Номенклатуре'";
			КонецЕсли;
		КонецЕсли; 	
		
		Если Объект.ВалютаУчета.Пустая() Тогда
			Объект.ВалютаУчета = Константы.ВалютаРегламентированногоУчетаОрганизаций.Получить();
			
			СтрокаСообщения = СтрокаСообщения + ?(ПустаяСтрока(СтрокаСообщения),"","; ") + "заполнен реквизит 'ВалютаУчета'";
		КонецЕсли;
		
		Если Объект.СтавкаНДС.Пустая() Тогда
			Объект.СтавкаНДС = Справочники.СтавкиНДС.ОсновнаяСтавкаНДС;
			
