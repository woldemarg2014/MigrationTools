|	МАКСИМУМ(Сумма),
|	МАКСИМУМ(СуммаНДС),
|	МАКСИМУМ(СуммаУпр)
|ПО
|	СкладКомпании,
|	ДокументПередачи,
|	Партия,
|	Автомобиль";

СтавкаБезНДС = Справочники.СтавкиНДС.БезНДС;
ТаблицаДанных.Очистить();

ПустойСклад = Справочники.СкладыКомпании.ПустаяСсылка();

ХозОперацияКомиссия = Справочники.ХозОперации.ВводОстатковАвтомобилейПереданныхНаКомиссию;

ВыборкаСклады = Запрос.Выполнить().Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
Пока ВыборкаСклады.Следующий() Цикл
	
	ТекСкладКомпании = ВыборкаСклады.СкладКомпании;
	
	ТекПодразделение = ТекСкладКомпании.Подразделение;
	ТекОрганизация   = ТекПодразделение.Организация;
	Розничный        = ТекСкладКомпании.Розничный;
	
	Если (ЗначениеЗаполнено(ТекПодразделение) И ТекПодразделение.ОсвобожденОтНДС) ИЛИ
		(ЗначениеЗаполнено(ТекОрганизация) И ТекОрганизация.ОсвобожденОтНДС) Тогда
		ОсвобожденОтНДС = Истина;
	Иначе
		ОсвобожденОтНДС = Ложь;
	КонецЕсли;
	
	ВыборкаДокументПередачи = ВыборкаСклады.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	Пока ВыборкаДокументПередачи.Следующий() Цикл
		
		ТекКонтрагент            = ВыборкаДокументПередачи.Контрагент;
		ТекДоговорВзаиморасчетов = ВыборкаДокументПередачи.ДоговорВзаиморасчетов;
		ТекДокументПередачи      = ВыборкаДокументПередачи.ДокументПередачи;
		
		Если ТекСкладКомпании = ПустойСклад Тогда
			ТекПодразделение = ТекДоговорВзаиморасчетов.Подразделение;
			ТекОрганизация   = ТекДоговорВзаиморасчетов.Организация;
		КонецЕсли;
		
		ВыборкаПартии = ВыборкаДокументПередачи.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
		Пока ВыборкаПартии.Следующий() Цикл
			ТекПартия = ВыборкаПартии.Партия;
			Если обЗначениеНеЗаполнено(ТекПартия) Тогда
				Продолжить;
			КонецЕсли;
			
			ВыборкаАвтомобиль = ВыборкаПартии.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
			Пока ВыборкаАвтомобиль.Следующий() Цикл
				
				ОстатокНаСкладе = ВыборкаАвтомобиль.Количество;
				ОстатокСумма    = ВыборкаАвтомобиль.Сумма;
				ОстатокСуммаНДС = ВыборкаАвтомобиль.СуммаНДС;
				ОстатокСуммаУпр = ВыборкаАвтомобиль.СуммаУпр;
				
				ВыборкаТаблицы  = ВыборкаАвтомобиль.Выбрать(ОбходРезультатаЗапроса.Прямой);
				Пока ВыборкаТаблицы.Следующий() Цикл
					
					Если ОстатокНаСкладе &lt;=0 Тогда
						Прервать;
					КонецЕсли;
					
					НоваяСтрока = ТаблицаДанных.Добавить();
					ЗаполнитьЗначенияСвойств(НоваяСтрока, ВыборкаТаблицы);
					НоваяСтрока.СуммаВсего = ВыборкаТаблицы.Сумма;
					
					Если ОсвобожденОтНДС Тогда
						НоваяСтрока.СтавкаНДС = СтавкаБезНДС;
						НоваяСтрока.Сумма     = НоваяСтрока.СуммаВсего;
					Иначе
						НоваяСтрока.СтавкаНДС = ВыборкаТаблицы.СтавкаНДС;
						// Рассчитаем значение реквизита "Сумма". Если расчет НДС не ведется, то ЦенаВключаетНДС будет всегда Истина. 
						НоваяСтрока.Сумма = НоваяСтрока.СуммаВсего;
					КонецЕсли;
					
					// Пересчитываем цену.
					Если НоваяСтрока.Количество &lt;&gt; 0 Тогда
						НоваяСтрока.Цена = Окр(НоваяСтрока.Сумма / НоваяСтрока.Количество, 2);
					КонецЕсли;
					
					ОстатокНаСкладе = ОстатокНаСкладе - НоваяСтрока.Количество;
					ОстатокСумма    = ОстатокСумма    - НоваяСтрока.СуммаВсего;
					ОстатокСуммаНДС = ОстатокСуммаНДС - НоваяСтрока.СуммаНДС;
					ОстатокСуммаУпр = ОстатокСуммаУпр - НоваяСтрока.СуммаУпр;
					
				КонецЦикла;
			КонецЦикла;
			
		КонецЦикла;
		
		Если ТаблицаДанных.Количество() &lt;&gt; 0 Тогда
			
			ИтогСумма    = ТаблицаДанных.Итог("СуммаВсего");
			ИтогСуммаУпр = ТаблицаДанных.Итог("СуммаУпр");
			
			Если ИтогСуммаУпр = 0 Тогда
				КурсВалютыУпрДокумента = КурсВалютыУпр;
			Иначе
				КурсВалютыУпрДокумента = ИтогСумма / ИтогСуммаУпр;
			КонецЕсли;
			
			ВходящиеДанные	= Новый Структура("Дата, ВалютаДокумента, ДоговорВзаиморасчетов, Комментарий, Контрагент, КурсВалютыУпр, КурсДокумента, Организация, ПодразделениеКомпании, СкладКомпании, ХозОперация, Автомобили");
			ВходящиеДанные.Дата						= НаДату;
			ВходящиеДанные.ВалютаДокумента			= ВалютаРегл;
			ВходящиеДанные.ДоговорВзаиморасчетов	= ТекДоговорВзаиморасчетов;
			ВходящиеДанные.Контрагент				= ТекКонтрагент;
			ВходящиеДанные.КурсВалютыУпр			= КурсВалютыУпрДокумента;
			ВходящиеДанные.КурсДокумента			= КурсДокумента;
			ВходящиеДанные.Организация				= ТекОрганизация;
			ВходящиеДанные.ПодразделениеКомпании	= ТекПодразделение;
			ВходящиеДанные.СкладКомпании            = Справочники.СкладыКомпании.ПустаяСсылка();
			ВходящиеДанные.ХозОперация				= ХозОперацияКомиссия;
			ВходящиеДанные.Комментарий				= "Перенос остатков автомобилей переданных на комиссию из Альфа-Авто 5. Партия-источник: " + Строка(ТекДокументПередачи);
