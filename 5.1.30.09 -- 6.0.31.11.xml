|	КонтактнаяИнформация.ТипКвартиры КАК ТипКвартиры,
|	КонтактнаяИнформация.Комментарий КАК Комментарий,
|	КонтактнаяИнформация.CRM_ПолеХраненияНомера КАК ПолеХраненияНомера,
|	КонтактнаяИнформация.ЗначениеПоУмолчанию КАК ОсновнойДляСвязи
|ИЗ
|	РегистрСведений.КонтактнаяИнформация КАК КонтактнаяИнформация
|ГДЕ
|	КонтактнаяИнформация.Объект = &amp;Контрагент");

Запрос.УстановитьПараметр("Контрагент", Источник.Ссылка);

Выборка = Запрос.Выполнить().Выбрать();
Пока Выборка.Следующий() Цикл
	НоваяСтрока = КоллекцияОбъектов.Добавить();
	ЗаполнитьЗначенияСвойств(НоваяСтрока, Выборка,, "Вид");
	
	Если Выборка.Тип = Перечисления.ТипыКонтактнойИнформации.Адрес Тогда
		
		Если ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Контрагенты") Тогда
			
			НоваяСтрока.Вид = ?(Выборка.Вид = Справочники.ВидыКонтактнойИнформации.АдресЮридический, "ЮрАдресКонтрагента", "ФактАдресКонтрагента");
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Организации") Тогда
			
			НоваяСтрока.Вид = ?(Выборка.Вид = Справочники.ВидыКонтактнойИнформации.АдресЮридический, "ЮрАдресОрганизации", "ФактАдресОрганизации");
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Сотрудники") Тогда
			
			НоваяСтрока.Вид = ?(Выборка.Вид = Справочники.ВидыКонтактнойИнформации.АдресЮридический, "ЮрАдресСотрудника", "ФактАдресСотрудника");
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.ПодразделенияКомпании") Тогда
			
			НоваяСтрока.Вид = ?(Выборка.Вид = Справочники.ВидыКонтактнойИнформации.АдресЮридический, "ЮрАдресПодразделенияКомпании", "ФактАдресПодразделенияКомпании");
			
		КонецЕсли;
		
		НоваяСтрока.ЗначенияПолей =
			"Индекс=" + Выборка.Поле1 + Символы.ПС
			+ "Регион=" + Выборка.Поле2 + Символы.ПС
			+ "Район=" + Выборка.Поле3 + Символы.ПС
			+ "Город=" + Выборка.Поле4 + Символы.ПС
			+ "НаселенныйПункт=" + Выборка.Поле5 + Символы.ПС
			+ "Улица=" + Выборка.Поле6 + Символы.ПС
			+ "Дом=" + Выборка.Поле7 + Символы.ПС
			+ "Корпус=" + Выборка.Поле8 + Символы.ПС
			+ "Квартира=" + Выборка.Поле9 + Символы.ПС
			+ "Этаж=" + Выборка.Поле11 + Символы.ПС
			+ "ГородскойРайон=" + Выборка.ГородскойРайон + Символы.ПС
			+ "ТипДома=" + Выборка.ТипДома + Символы.ПС
			+ "ТипКорпуса=" + Выборка.ТипКорпуса + Символы.ПС
			+ "ТипКвартиры=" + Выборка.ТипКвартиры + Символы.ПС
			+ "Комментарий=" + Выборка.Комментарий;
		
	ИначеЕсли Выборка.Тип = Перечисления.ТипыКонтактнойИнформации.Телефон Тогда
		
		Если Выборка.Вид = Справочники.ВидыКонтактнойИнформации.Факс Тогда
			
			//НоваяСтрока.Тип = "Телефон";
			
			Если ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Контрагенты") Тогда
				
				НоваяСтрока.Вид = "ФаксКонтрагента";
				
			ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Организации") Тогда
				
				НоваяСтрока.Вид = "ФаксОрганизации";
				
			ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Сотрудники") Тогда
				
				НоваяСтрока.Вид = "ФаксСотрудника";
				
			ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.ПодразделенияКомпании") Тогда
				
				НоваяСтрока.Вид = "ФаксПодразделенияКомпании";
				
			КонецЕсли;
			
		Иначе
			
			Если ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Контрагенты") Тогда
				
				НоваяСтрока.Вид = "ТелефонКонтрагента";
				
			ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Организации") Тогда
				
				НоваяСтрока.Вид = "ТелефонОрганизации";
				
			ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Сотрудники") Тогда
				
				НоваяСтрока.Вид = "ТелефонСотрудника";
				
			ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.ПодразделенияКомпании") Тогда
				
				НоваяСтрока.Вид = "ТелефонПодразделенияКомпании";
				
			КонецЕсли;
			
		КонецЕсли;
		
		НоваяСтрока.ЗначенияПолей = ?(ЗначениеЗаполнено(Выборка.Поле1), "КодСтраны="     + Выборка.Поле1 + Символы.ПС, "")
		   + ?(ЗначениеЗаполнено(Выборка.Поле2), "КодГорода="     + Выборка.Поле2 + Символы.ПС, "")
		   + ?(ЗначениеЗаполнено(Выборка.Поле3), "НомерТелефона=" + Выборка.Поле3 + Символы.ПС, "")
		   + ?(ЗначениеЗаполнено(Выборка.Поле4), "Добавочный="    + Выборка.Поле4, "");
		
		НоваяСтрока.НомерТелефонаБезКодов = Выборка.Поле3;
		НоваяСтрока.НомерТелефона         = Выборка.Поле1 + Выборка.Поле2 + Выборка.Поле3;
		
	ИначеЕсли Выборка.Тип = Перечисления.ТипыКонтактнойИнформации.АдресЭлектроннойПочты Тогда
		
		Если ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Контрагенты") Тогда
			
			НоваяСтрока.Вид = "EmailКонтрагента";
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Организации") Тогда
			
			НоваяСтрока.Вид = "EmailОрганизации";
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Сотрудники") Тогда
			
			НоваяСтрока.Вид = "EmailСотрудника";
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.ПодразделенияКомпании") Тогда
			
			НоваяСтрока.Вид = "EmailПодразделенияКомпании";
			
		КонецЕсли;
		
		НоваяСтрока.АдресЭП = Выборка.Представление;
		
	ИначеЕсли Выборка.Тип = Перечисления.ТипыКонтактнойИнформации.ВебСтраница Тогда
		
		Если ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Контрагенты") Тогда
			
			НоваяСтрока.Вид = "ВебСтраницаКонтрагента";
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Организации") Тогда
			
			НоваяСтрока.Вид = "ВебСтраницаОрганизации";
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Сотрудники") Тогда
			
			НоваяСтрока.Вид = "ВебСтраницаСотрудника";
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.ПодразделенияКомпании") Тогда
			
			НоваяСтрока.Вид = "ВебСтраницаПодразделенияКомпании";
			
		КонецЕсли;
		
	ИначеЕсли Выборка.Тип = Перечисления.ТипыКонтактнойИнформации.Другое Тогда
		
		Если ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Контрагенты") Тогда
			
			НоваяСтрока.Вид = "ДругаяИнформацияКонтрагента";
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Организации") Тогда
			
			НоваяСтрока.Вид = "ДругаяИнформацияОрганизации";
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.Сотрудники") Тогда
			
			НоваяСтрока.Вид = "ДругаяИнформацияСотрудника";
			
		ИначеЕсли ТипЗнч(Источник.Ссылка) = Тип("СправочникСсылка.ПодразделенияКомпании") Тогда
			
			НоваяСтрока.Вид = "ДругаяИнформацияПодразделенияКомпании";
			
		КонецЕсли;
	
	КонецЕсли;
	
КонецЦикла;</Текст>
		</Алгоритм>
		<Алгоритм Имя="ПроверитьДублиКодовСправочникаВБазеИсточнике" ИспользуетсяПриЗагрузке="false">
			<Текст>ЕстьДубли = Ложь;
#Если Клиент Тогда
	Состояние("Проверка уникальности кодов справочника '" + ИмяСправочника + "'...");
#КонецЕсли
Запрос = Новый Запрос("ВЫБРАТЬ ПЕРВЫЕ 5
                      |	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ " + ИмяСправочника + ".Ссылка) КАК КоличествоДублей,
                      |	" + ИмяСправочника + ".Код КАК Код
                      |ИЗ
                      |	Справочник." + ИмяСправочника + " КАК " + ИмяСправочника + "
                      |
                      |СГРУППИРОВАТЬ ПО
                      |	" + ИмяСправочника + ".Код
                      |
                      |УПОРЯДОЧИТЬ ПО
                      |	КоличествоДублей УБЫВ");
РезультатЗапроса = Запрос.Выполнить();
Если НЕ РезультатЗапроса.Пустой() Тогда
	Выборка = РезультатЗапроса.Выбрать();
	Выборка.Следующий();
	КоличествоДублей = Выборка.КоличествоДублей;
	Если КоличествоДублей &gt; 1 Тогда
		ЕстьДубли = Истина;
		СтруктураЗаписи = Новый Структура;
		СтруктураЗаписи.Вставить(ИмяСправочника, "Максимальное количество дублей = " + Формат(КоличествоДублей, "ЧГ=0"));
		СтрокаСообщения = "Проблема в источнике. Выгрузка запрещена! 
		|В справочнике '" + ИмяСправочника + "' есть элементы с одинаковыми кодами. 
		|Перед выгрузкой нужно выполнить перенумерацию кодов справочника '" + ИмяСправочника + "'.
		|Это можно сделать вручную или с помощью обработки 'УниверсальныеПодборИОбработкаОбъектов.epf', которую можно найти на сайте ИТС (https://its.1c.ru/db/metod8dev/content/3686/hdoc)";
		ОбщиеПроцедурыФункции.ЗаписатьВПротоколВыполнения(СтрокаСообщения, СтруктураЗаписи, Истина);
	КонецЕсли; 	
КонецЕсли;</Текст>
