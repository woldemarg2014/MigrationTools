|		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ЗаказыРаспределение.Остатки(&amp;ДатаСведений, ЗаказПоставщика ССЫЛКА Документ.ЗаказВнутренний) КАК ЗаказыРаспределение
|		ПО ТаблицаЗаказов.Заказ = ЗаказыРаспределение.ЗаказПоставщика
|			И ТаблицаЗаказов.Номенклатура = ЗаказыРаспределение.Номенклатура
|			И ТаблицаЗаказов.ХарактеристикаНоменклатуры = ЗаказыРаспределение.ХарактеристикаНоменклатуры
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЗаказВнутренний КАК ЗаказВнутренний
|		ПО ТаблицаЗаказов.Заказ = ЗаказВнутренний.Ссылка
|ИТОГИ
|	МАКСИМУМ(СкладКомпании),
|	МАКСИМУМ(Количество),
|	МАКСИМУМ(Цена),
|	СУММА(Распределено),
|	МАКСИМУМ(Организация),
|	МАКСИМУМ(ПодразделениеКомпании),
|	МАКСИМУМ(ТипЦен),
|	МАКСИМУМ(ПодразделениеПолучатель),
|	МАКСИМУМ(ВалютаДокумента)
|ПО
|	Заказ,
|	Номенклатура,
|	ХарактеристикаНоменклатуры";

СтавкаБезНДС = Справочники.СтавкиНДС.БезНДС;
ХозОперация  = Справочники.ХозОперации.ЗаказВнутренний;

ВалютаУпр      = Константы.ВалютаУправленческогоУчетаКомпании.Получить();
СтруктураКурса = обКурсДляВалюты(ВалютаУпр, НаДату);
КурсВалютыУпр  = СтруктураКурса.Курс / ?(СтруктураКурса.Кратность = 0, 1, СтруктураКурса.Кратность);

ВалютаРегл     = Константы.ВалютаРегламентированногоУчетаОрганизаций.Получить();
СтруктураКурса = обКурсДляВалюты(ВалютаРегл,	НаДату);
КурсРегл       = СтруктураКурса.Курс / ?(СтруктураКурса.Кратность = 0, 1, СтруктураКурса.Кратность);

ВыборкаЗаказов = Запрос.Выполнить().Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
Пока ВыборкаЗаказов.Следующий() Цикл
	
	ТекПодразделение = ВыборкаЗаказов.ПодразделениеКомпании;
	ТекОрганизация   = ВыборкаЗаказов.Организация;
	ТекЗаказ         = ВыборкаЗаказов.Заказ;
	ТекТипЦен        = ВыборкаЗаказов.ТипЦен;
	
	ВалютаДокумента = ВыборкаЗаказов.ВалютаДокумента;
	СтруктураКурса  = обКурсДляВалюты(ВалютаДокумента, НаДату);
	КурсДокумента   = СтруктураКурса.Курс / ?(СтруктураКурса.Кратность = 0, 1, СтруктураКурса.Кратность);
	
	ТаблицаТовары.Очистить();
	ТаблицаРаспределение.Очистить();
	
	НомерЗаписи    = 1;
	
	ВыборкаНоменклатура = ВыборкаЗаказов.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	Пока ВыборкаНоменклатура.Следующий() Цикл
		
		ВыборкаХарактеристик = ВыборкаНоменклатура.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
		Пока ВыборкаХарактеристик.Следующий() Цикл
			
			НоваяСтрока = ТаблицаТовары.Добавить();
			ЗаполнитьЗначенияСвойств(НоваяСтрока, ВыборкаХарактеристик);
			
			Если ВыборкаХарактеристик.Коэффициент = 1 ИЛИ ВыборкаХарактеристик.Коэффициент = 0 Тогда
				НоваяСтрока.Количество   = ВыборкаХарактеристик.Количество;
				НоваяСтрока.Распределено = ВыборкаХарактеристик.Распределено;
			Иначе
				НоваяСтрока.Количество   = Окр(ВыборкаХарактеристик.Количество/ВыборкаХарактеристик.Коэффициент, 3);
				НоваяСтрока.Распределено = Окр(ВыборкаХарактеристик.Распределено/ВыборкаХарактеристик.Коэффициент, 3);
			КонецЕсли;
			
			НоваяСтрока.Сумма      = Окр(НоваяСтрока.Цена*ВыборкаХарактеристик.Количество, 2);
			НоваяСтрока.СуммаВсего = НоваяСтрока.Сумма;
			
			Если ВалютаДокумента&lt;&gt;ВалютаУпр Тогда
				НоваяСтрока.СуммаВсего = обПересчет(НоваяСтрока.СуммаВсего, ВалютаУпр, КурсВалютыУпр, ВалютаДокумента, КурсДокумента);
			КонецЕсли;
			
			//ТаблицаТовары.Колонки.Добавить("ИдентификаторСтроки");
			ВыборкаРаспределения = ВыборкаХарактеристик.Выбрать();
			Пока ВыборкаРаспределения.Следующий() Цикл
				
				Если ВыборкаРаспределения.Распределено &lt;= 0 Тогда
					Продолжить;
				КонецЕсли;
				
				НоваяСтрокаРаспределение = ТаблицаРаспределение.Добавить();
				ЗаполнитьЗначенияСвойств(НоваяСтрокаРаспределение, ВыборкаРаспределения,,"Цена");
				
				Если ВыборкаРаспределения.Коэффициент = 1 ИЛИ ВыборкаРаспределения.Коэффициент = 0 Тогда
					НоваяСтрокаРаспределение.Количество = ВыборкаРаспределения.Распределено;
				Иначе
					НоваяСтрокаРаспределение.Количество = Окр(ВыборкаРаспределения.Распределено/ВыборкаРаспределения.Коэффициент, 3);
				КонецЕсли;
				
				НоваяСтрокаРаспределение.Цена = 0;
				
			КонецЦикла;
			
		КонецЦикла;
		
	КонецЦикла;
	
	Если ТаблицаТовары.Количество() &lt;&gt; 0 Тогда
		
		//ВходящиеДанные = Новый Структура("Номер, Дата, Автор, ВалютаДокумента, ДатаОперации, ДатаСоздания, Комментарий, КурсВалютыУпр, КурсДокумента, Организация, ПодразделениеКомпании, ПодразделениеПолучатель, СкладКомпании, СуммаДокумента, ТипЦен, ХозОперация, Номер, ПометкаУдаления, Проведен, РаспределениеЗаказа, Идентификатор, Товары");
		ВходящиеДанные = Новый Структура("ВалютаДокумента, Комментарий, КурсВалютыУпр, КурсДокумента, СкладКомпании, ХозОперация, РаспределениеЗаказа, Товары");
		ВходящиеДанные.Вставить("Номер", ТекЗаказ.Номер);
		ВходящиеДанные.Вставить("Дата" , ТекЗаказ.Дата);
		ВходящиеДанные.ВалютаДокумента         = ВалютаДокумента;
		ВходящиеДанные.Комментарий             = ""+Строка(ТекЗаказ)+". Перенос внутренних заказов товара из Альфа-Авто 5";
		ВходящиеДанные.КурсВалютыУпр           = КурсВалютыУпр;
		ВходящиеДанные.КурсДокумента           = КурсДокумента;
		ВходящиеДанные.Вставить("Организация"            , ТекОрганизация);
		ВходящиеДанные.Вставить("ПодразделениеКомпании"  , ТекПодразделение);
		ВходящиеДанные.Вставить("ПодразделениеПолучатель", ВыборкаЗаказов.ПодразделениеПолучатель);
		ВходящиеДанные.Вставить("ТипЦен"                 , ТекТипЦен);
		ВходящиеДанные.СкладКомпании           = ВыборкаЗаказов.СкладКомпании;
		//ВходящиеДанные.СуммаДокумента          = 0;
		ВходящиеДанные.ХозОперация             = ХозОперация;
		//ВходящиеДанные.ПометкаУдаления         = Ложь;
		//ВходящиеДанные.Проведен                = Ложь;
		ВходящиеДанные.Товары                  = ТаблицаТовары;
		ВходящиеДанные.РаспределениеЗаказа     = ТаблицаРаспределение;
		
		//***
		ВыгрузитьПоПравилу(,, ВходящиеДанные,, "ОстаткиЗаказовВнутренних");
		ТаблицаТовары.Очистить();
		ТаблицаРаспределение.Очистить();
		
	КонецЕсли;
	
КонецЦикла;</ПередОбработкойПравила>
