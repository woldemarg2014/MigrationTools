				Если ВыбокаЗапись.СуммаУпр &lt;&gt; 0 Тогда
				    НоваяСтрока.ВыручкаДоПримененияСкидкиБезНДСУпр = Окр(НоваяСтрока.ВыручкаДоПримененияСкидкиУпр*НоваяСтрока.ВыручкаБезНДСУпр/ВыбокаЗапись.СуммаУпр,2); //
				Иначе
					НоваяСтрока.ВыручкаДоПримененияСкидкиБезНДСУпр = 0;
				КонецЕсли;
				Если ВыбокаЗапись.Себестоимость &lt;&gt; 0 Тогда
				    НоваяСтрока.СуммаНДСВходящийУпр = Окр(ВыбокаЗапись.СуммаНДСВходящий*ВыбокаЗапись.СебестоимостьУпр/ВыбокаЗапись.Себестоимость,2); //
				Иначе
					НоваяСтрока.СуммаНДСВходящийУпр = 0;
				КонецЕсли;
				НоваяСтрока.СебестоимостьБезНДСУпр = ВыбокаЗапись.СебестоимостьУпр - НоваяСтрока.СуммаНДСВходящийУпр; //

				
				ВходящиеДанные = Новый Структура("Дата, Комментарий, Организация, ПодразделениеКомпании, Продажи");
				ВходящиеДанные.Дата						= ВыбокаЗапись.Период;
				ВходящиеДанные.Организация				= ТекОрганизация;
				ВходящиеДанные.ПодразделениеКомпании	= ТекПодразделение;
				ВходящиеДанные.Комментарий				= "Перенос истории продаж из Альфа-Авто 5. Неделя: " + Формат(ВходящиеДанные.Дата,"ДЛФ=D");
				ВходящиеДанные.Продажи					= ТаблицаДанных;
				
			КонецЦикла;
			
			// заполним документ недельными продажами
			ВыгрузитьПоПравилу(,, ВходящиеДанные,, "ЗагрузкаИсторииПродажНоменклатуры");
			ТаблицаДанных.Очистить();
		КонецЦикла;
	КонецЦикла;
КонецЦикла;</ПередОбработкойПравила>
			</Правило>
		</Группа>
	</ПравилаВыгрузкиДанных>
	<ПравилаОчисткиДанных/>
	<Алгоритмы>
		<Алгоритм Имя="ДопРеквизиты" ИспользуетсяПриЗагрузке="false">
			<Текст>Запрос = Новый Запрос("ВЫБРАТЬ
|	ЗначенияСвойствОбъектов.Свойство КАК Свойство,
|	ЗначенияСвойствОбъектов.Значение КАК Значение,
|	ЗначенияСвойствОбъектов.Свойство.Наименование КАК ТекстоваяСтрока
|ИЗ
|	РегистрСведений.ЗначенияСвойствОбъектов КАК ЗначенияСвойствОбъектов
|ГДЕ
|	ЗначенияСвойствОбъектов.Объект = &amp;Объект");

Запрос.УстановитьПараметр("Объект", Источник.Ссылка);
КоллекцияОбъектов = Запрос.Выполнить().Выгрузить();</Текст>
		</Алгоритм>
		<Алгоритм Имя="ОбработатьКонтактнуюИнформацию" ИспользуетсяПриЗагрузке="true">
			<Текст>Для Каждого Строка Из Объект.КонтактнаяИнформация Цикл
	Попытка
		ЗначенияПолей = УправлениеКонтактнойИнформацией.КонтактнаяИнформацияВXML("", Строка.Представление, Строка.Вид);
	Исключение
		ЗначенияПолей = "";
	КонецПопытки;
	
	Если Строка.Тип = Перечисления.ТипыКонтактнойИнформации.Телефон тогда
		ТегТелефона   = СтрШаблон("Номер=""%1""", XMLСтрока(Строка.Представление));
		ЗначенияПолей = СтрЗаменить(ЗначенияПолей, "Номер=""""", ТегТелефона);
	КонецЕсли;
	
	Если Строка.Тип = Перечисления.ТипыКонтактнойИнформации.Адрес тогда
		
		МассивПолей = Новый Массив;
		МассивПолей.Добавить("Индекс");
		МассивПолей.Добавить("Регион");
		МассивПолей.Добавить("Район");
		МассивПолей.Добавить("Город");
		МассивПолей.Добавить("НаселенныйПункт");
		МассивПолей.Добавить("Улица");
		МассивПолей.Добавить("Дом");
		МассивПолей.Добавить("Корпус");
		МассивПолей.Добавить("Квартира");
		МассивПолей.Добавить("Этаж");
		МассивПолей.Добавить("ГородскойРайон");
		МассивПолей.Добавить("ТипДома");
		МассивПолей.Добавить("ТипКорпуса");
		МассивПолей.Добавить("ТипКвартиры");
		МассивПолей.Добавить("Комментарий");
		
		СтруктураАдреса = Новый Структура;
		Для Каждого ИмяПоля Из МассивПолей Цикл
			СтруктураАдреса.Вставить(ИмяПоля, "");
		КонецЦикла;
		
		Если НЕ ПустаяСтрока(Строка.ЗначенияПолей) Тогда
			
			Для Сч = 1 По СтрЧислоСтрок(Строка.ЗначенияПолей) Цикл
				
				СтрокаАдреса = СтрПолучитьСтроку(Строка.ЗначенияПолей, Сч);
				
				Для Каждого ИмяПоля Из МассивПолей Цикл
					
					Если СтрНачинаетсяС(СтрокаАдреса, ИмяПоля + "=") Тогда
						СтруктураАдреса[ИмяПоля] = Сред(СтрокаАдреса, СтрДлина(ИмяПоля)+2);
						Прервать;
					КонецЕсли;
					
				КонецЦикла;
				
			КонецЦикла;
			
		КонецЕсли;
		
		ЗначенияПолей = СтрШаблон(
			"&lt;КонтактнаяИнформация xmlns=""http://www.v8.1c.ru/ssl/contactinfo"" xmlns:xs=""http://www.w3.org/2001/XMLSchema"" xmlns:xsi=""http://www.w3.org/2001/XMLSchema-instance"" Представление=""%1""&gt;",
			XMLСтрока(Строка.Представление)
		);
		
		Если ЗначениеЗаполнено(СтруктураАдреса.Комментарий) Тогда
			ЗначенияПолей = ЗначенияПолей + Символы.ПС + СтрШаблон("&lt;Комментарий&gt;%1&lt;/Комментарий&gt;", XMLСтрока(СтруктураАдреса.Комментарий));
		Иначе
			ЗначенияПолей = ЗначенияПолей + Символы.ПС + "&lt;Комментарий/&gt;";
		КонецЕсли;
		
		ЗначенияПолей = ЗначенияПолей + "
		|	&lt;Состав xsi:type=""Адрес"" Страна=""РОССИЯ""&gt;
		|		&lt;Состав xsi:type=""АдресРФ""&gt;";
		
		Если ЗначениеЗаполнено(СтруктураАдреса.Регион)
			ИЛИ ЗначениеЗаполнено(СтруктураАдреса.Район)
			ИЛИ ЗначениеЗаполнено(СтруктураАдреса.Город)
			ИЛИ ЗначениеЗаполнено(СтруктураАдреса.ГородскойРайон)
			ИЛИ ЗначениеЗаполнено(СтруктураАдреса.НаселенныйПункт)
			ИЛИ ЗначениеЗаполнено(СтруктураАдреса.Улица)
			ИЛИ ЗначениеЗаполнено(СтруктураАдреса.Дом)
			ИЛИ ЗначениеЗаполнено(СтруктураАдреса.Корпус)
			ИЛИ ЗначениеЗаполнено(СтруктураАдреса.Квартира)
			ИЛИ ЗначениеЗаполнено(СтруктураАдреса.Этаж) Тогда
			
			Если ЗначениеЗаполнено(СтруктураАдреса.Регион) Тогда
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + СтрШаблон("&lt;СубъектРФ&gt;%1&lt;/СубъектРФ&gt;", XMLСтрока(СтруктураАдреса.Регион));
			Иначе
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + "&lt;СубъектРФ/&gt;";
			КонецЕсли;
			
			ЗначенияПолей = ЗначенияПолей + Символы.ПС + "&lt;Округ/&gt;";
			
			Если ЗначениеЗаполнено(СтруктураАдреса.Район) Тогда
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + СтрШаблон("&lt;СвРайМО&gt;
																		|	&lt;Район&gt;%1&lt;/Район&gt;
																		|&lt;/СвРайМО&gt;", XMLСтрока(СтруктураАдреса.Район));
			Иначе
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + "&lt;СвРайМО&gt;
															|	&lt;Район/&gt;
															|&lt;/СвРайМО&gt;";
			КонецЕсли;
			
			Если ЗначениеЗаполнено(СтруктураАдреса.Город) Тогда
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + СтрШаблон("&lt;Город&gt;%1&lt;/Город&gt;", XMLСтрока(СтруктураАдреса.Город));
			Иначе
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + "&lt;Город/&gt;";
			КонецЕсли;
			
			Если ЗначениеЗаполнено(СтруктураАдреса.ГородскойРайон) Тогда
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + СтрШаблон("&lt;ВнутригРайон&gt;%1&lt;/ВнутригРайон&gt;", XMLСтрока(СтруктураАдреса.ГородскойРайон));
			Иначе
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + "&lt;ВнутригРайон/&gt;";
			КонецЕсли;
			
			Если ЗначениеЗаполнено(СтруктураАдреса.НаселенныйПункт) Тогда
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + СтрШаблон("&lt;НаселПункт&gt;%1&lt;/НаселПункт&gt;", XMLСтрока(СтруктураАдреса.НаселенныйПункт));
			Иначе
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + "&lt;НаселПункт/&gt;";
			КонецЕсли;
			
			Если ЗначениеЗаполнено(СтруктураАдреса.Улица) Тогда
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + СтрШаблон("&lt;Улица&gt;%1&lt;/Улица&gt;", XMLСтрока(СтруктураАдреса.Улица));
			Иначе
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + "&lt;Улица/&gt;";
			КонецЕсли;
			
			ЗначенияПолей = ЗначенияПолей + Символы.ПС + "&lt;ОКТМО&gt;0&lt;/ОКТМО&gt;";
			
			Если ЗначениеЗаполнено(СтруктураАдреса.Дом) Тогда
				
				ТипДома = "1010";
				Если ВРег(СтруктураАдреса.ТипДома) = ВРег("владение") Тогда
					ТипДома = "1020";
				ИначеЕсли ВРег(СтруктураАдреса.ТипДома) = ВРег("Домовладение") Тогда
					ТипДома = "1030";
				КонецЕсли;
				
				ТекстДома = 
				СтрШаблон(
				"&lt;ДопАдрЭл&gt;
				|	&lt;Номер Тип=""%1"" Значение=""%2""/&gt;
				|&lt;/ДопАдрЭл&gt;",
				XMLСтрока(ТипДома),
				XMLСтрока(СтруктураАдреса.Дом)
				);
				
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + ТекстДома;
				
			КонецЕсли;
			
			Если ЗначениеЗаполнено(СтруктураАдреса.Корпус) Тогда
				
				ТипКорпуса = "1050";
				Если ВРег(СтруктураАдреса.ТипКорпуса) = ВРег("строение") Тогда
					ТипКорпуса = "1060";
				ИначеЕсли ВРег(СтруктураАдреса.ТипКорпуса) = ВРег("литера") Тогда
					ТипКорпуса = "1080";
				ИначеЕсли ВРег(СтруктураАдреса.ТипКорпуса) = ВРег("сооружение") Тогда
					ТипКорпуса = "1070";
				ИначеЕсли ВРег(СтруктураАдреса.ТипКорпуса) = ВРег("участок") Тогда
					ТипКорпуса = "1040";
				КонецЕсли;
				
				ТекстКорпуса = 
				СтрШаблон(
				"&lt;ДопАдрЭл&gt;
				|	&lt;Номер Тип=""%1"" Значение=""%2""/&gt;
				|&lt;/ДопАдрЭл&gt;",
				XMLСтрока(ТипКорпуса),
				XMLСтрока(СтруктураАдреса.Корпус)
				);
				
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + ТекстКорпуса;
				
			КонецЕсли;
			
			Если ЗначениеЗаполнено(СтруктураАдреса.Квартира) Тогда
				
				ТипКвартиры = "2010";
				Если ВРег(СтруктураАдреса.ТипКвартиры) = ВРег("оф.") Тогда
					ТипКвартиры = "2030";
				ИначеЕсли ВРег(СтруктураАдреса.ТипКвартиры) = ВРег("бокс") Тогда
					ТипКвартиры = "2040";
				ИначеЕсли ВРег(СтруктураАдреса.ТипКвартиры) = ВРег("пом.") Тогда
					ТипКвартиры = "2020";
				ИначеЕсли ВРег(СтруктураАдреса.ТипКвартиры) = ВРег("к.") Тогда
					ТипКвартиры = "2050";
				КонецЕсли;
				
				ТекстКвартиры = 
				СтрШаблон(
				"&lt;ДопАдрЭл&gt;
				|	&lt;Номер Тип=""%1"" Значение=""%2""/&gt;
				|&lt;/ДопАдрЭл&gt;",
				XMLСтрока(ТипКвартиры),
				XMLСтрока(СтруктураАдреса.Квартира)
				);
				
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + ТекстКвартиры;
				
			КонецЕсли;
			
			Если ЗначениеЗаполнено(СтруктураАдреса.Этаж) Тогда
				
				ТекстЭтажа = 
				СтрШаблон(
				"&lt;ДопАдрЭл&gt;
				|	&lt;Номер Тип=""2060"" Значение=""%1""/&gt;
				|&lt;/ДопАдрЭл&gt;",
				XMLСтрока(СтруктураАдреса.Этаж)
				);
				
				ЗначенияПолей = ЗначенияПолей + Символы.ПС + ТекстЭтажа;
				
			КонецЕсли;
			
			ЗначенияПолей = ЗначенияПолей + Символы.ПС +
			"&lt;ДопАдрЭл ТипАдрЭл=""10100000"" Значение=""443901""/&gt;";
			
		Иначе
			
			ЗначенияПолей = ЗначенияПолей + Символы.ПС +
			СтрШаблон("&lt;ОКТМО&gt;0&lt;/ОКТМО&gt;
			|&lt;ДопАдрЭл ТипАдрЭл=""10100000"" Значение=""""/&gt;
			|&lt;Адрес_по_документу&gt;%1&lt;/Адрес_по_документу&gt;",
			XMLСтрока(Строка.Представление)
			);
			
		КонецЕсли;
		
		ЗначенияПолей = ЗначенияПолей + Символы.ПС +
		"&lt;/Состав&gt;
		|&lt;/Состав&gt;
		|&lt;/КонтактнаяИнформация&gt;";
		
	КонецЕсли;
	
	Строка.ЗначенияПолей = ЗначенияПолей;
	
КонецЦикла;</Текст>
		</Алгоритм>
		<Алгоритм Имя="ПереносКИ" ИспользуетсяПриЗагрузке="false">
			<Текст>КоллекцияОбъектов = Новый ТаблицаЗначений;
КоллекцияОбъектов.Колонки.Добавить("ОсновнойДляСвязи");
КоллекцияОбъектов.Колонки.Добавить("Тип");
КоллекцияОбъектов.Колонки.Добавить("Вид");
КоллекцияОбъектов.Колонки.Добавить("Представление");
КоллекцияОбъектов.Колонки.Добавить("ЗначенияПолей");
КоллекцияОбъектов.Колонки.Добавить("Страна");
КоллекцияОбъектов.Колонки.Добавить("Регион");
КоллекцияОбъектов.Колонки.Добавить("Город");
КоллекцияОбъектов.Колонки.Добавить("АдресЭП");
КоллекцияОбъектов.Колонки.Добавить("ДоменноеИмяСервера");
КоллекцияОбъектов.Колонки.Добавить("НомерТелефона");
КоллекцияОбъектов.Колонки.Добавить("НомерТелефонаБезКодов");

Запрос = Новый Запрос("
|ВЫБРАТЬ
|	КонтактнаяИнформация.Тип КАК Тип,
|	КонтактнаяИнформация.Вид КАК Вид,
|	КонтактнаяИнформация.Представление КАК Представление,
|	КонтактнаяИнформация.Поле1 КАК Поле1, // Вспомогательное поле: Код страны или Индекс
|	КонтактнаяИнформация.Поле2 КАК Поле2, // Вспомогательное поле: Код города или Регион
