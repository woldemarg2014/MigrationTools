				Если ЦенаВключаетНДС Тогда
					НоваяСтрока.Сумма = НоваяСтрока.СуммаВсего;
				Иначе
					НоваяСтрока.Сумма = НоваяСтрока.СуммаВсего - НоваяСтрока.СуммаНДС;
				КонецЕсли;
