		ВыборкаХарактеристик = ВыборкаНоменклатура.Выбрать();
		Пока ВыборкаХарактеристик.Следующий() Цикл
			
			НоваяСтрока = ТаблицаДанных.Добавить();
			ЗаполнитьЗначенияСвойств(НоваяСтрока, ВыборкаХарактеристик);
			
			Если ВыборкаХарактеристик.Коэффициент = 1 ИЛИ ВыборкаХарактеристик.Коэффициент = 0 Тогда
				НоваяСтрока.Количество = ВыборкаХарактеристик.Количество;
				НоваяСтрока.Резерв     = ВыборкаХарактеристик.Резерв;
			Иначе
				НоваяСтрока.Количество = Окр(ВыборкаХарактеристик.Количество/ВыборкаХарактеристик.Коэффициент, 3);
				НоваяСтрока.Резерв     = Окр(ВыборкаХарактеристик.Резерв/ВыборкаХарактеристик.Коэффициент, 3);
			КонецЕсли;
			
			НоваяСтрока.СуммаВсего = ВыборкаХарактеристик.Сумма;
			
			Если ВалютаДокумента&lt;&gt;ВалютаРегл Тогда
				НоваяСтрока.СуммаВсего = обПересчет(НоваяСтрока.СуммаВсего, ВалютаРегл, КурсРегл, ВалютаДокумента, КурсДокумента);
			КонецЕсли;
			
			Если ОсвобожденОтНДС Тогда
				НоваяСтрока.СтавкаНДС = СтавкаБезНДС;
