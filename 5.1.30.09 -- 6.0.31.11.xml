		ТипЦенЗН        = ВыборкаЗН.ТипЦен;
		ДатаНачалаЗН    = ВыборкаЗН.ДатаНачала;
		Если ТекЗаказНаряд = ПустойЗН Тогда
			ЦенаВключаетНДС = Истина;
		Иначе
			ЦенаВключаетНДС = (ТипЦенЗН.Пустая() ИЛИ ТипЦенЗН.ЦенаВключаетНДС);
		КонецЕсли;
		
		ВыборкаПартии = ВыборкаЗН.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
		Пока ВыборкаПартии.Следующий() Цикл
			
			ТекПартия = ВыборкаПартии.Партия;
			Если обЗначениеНеЗаполнено(ТекПартия) Тогда
				Продолжить;
			КонецЕсли;
			
			Попытка
				ТекКонтрагент = ТекПартия.Контрагент;
			Исключение
				ТекКонтрагент = Неопределено;
			КонецПопытки;
			
			Попытка
				ТекДоговорВзаиморасчетов = ТекПартия.ДоговорВзаиморасчетов;
			Исключение
				ТекДоговорВзаиморасчетов = Неопределено;
			КонецПопытки;
		
			ВыборкаНоменклатура = ВыборкаПартии.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
			Пока ВыборкаНоменклатура.Следующий() Цикл
				
				Если ВыборкаНоменклатура.ЕдиницаИзмерения = ПустаяЕдиница Тогда
					
					//++gavv - проблема когда у номенклатуры не заполнена основная единица измерения (сохранил логирование, сам такую проблему не видел)
					СтрокаСообщения = "У номенклатуры: "+Строка(ВыборкаНоменклатура.Номенклатура)+" не заполнена основная единица, остаток НЗП по заказ-наряду: " + Строка(ТекЗаказНаряд) + " перенесен не будет!";
					СтруктураЗаписи = Новый Структура;
					СтруктураЗаписи.Вставить("ЗаказНаряд", Строка(ТекЗаказНаряд));
					ОбщиеПроцедурыФункции.ЗаписатьВПротоколВыполнения("Проблема переноса НЗП: " + СтрокаСообщения, СтруктураЗаписи, Ложь);
					Продолжить;
				КонецЕсли;
				
				ВыборкаХарактеристик = ВыборкаНоменклатура.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
				Пока ВыборкаХарактеристик.Следующий() Цикл
					
					ОстатокНаСкладе = ВыборкаХарактеристик.Количество;
					ВыборкаТаблицы  = ВыборкаХарактеристик.Выбрать(ОбходРезультатаЗапроса.Прямой);
					
					ОстатокСумма    = ВыборкаХарактеристик.Сумма;
					ОстатокСуммаНДС = ВыборкаХарактеристик.СуммаНДС;
					ОстатокСуммаУпр = ВыборкаХарактеристик.СуммаУпр;
					
					Пока ВыборкаТаблицы.Следующий() Цикл
						
						ВсеОК = Истина;
						
						Если ОстатокНаСкладе &lt;=0 Тогда
							Прервать;
						КонецЕсли;
						
						Если ВыборкаТаблицы.Количество&lt;=0 Тогда
							
							//++gavv - проблема в остатках НЗП есть отрицательные остатки (сохранил логирование, сам такую проблему не видел)
							СтрокаСообщения = ?(ВыборкаТаблицы.Количество = 0, "Нулевые ", "Отрицательные ") + "остатки номенклатуры в НЗП! Заказ-Наряд: " + Строка(ТекЗаказНаряд) + " ;Номенклатура: " + СокрЛП(ВыборкаТаблицы.Номенклатура) + "; Количество: " + Формат(ВыборкаТаблицы.Количество, "ЧДЦ=2; ЧГ=0") + ". Остаток НЗП перенесен не будет!";
							СтруктураЗаписи = Новый Структура;
							СтруктураЗаписи.Вставить("ЗаказНаряд", Строка(ТекЗаказНаряд));
							ОбщиеПроцедурыФункции.ЗаписатьВПротоколВыполнения("Проблема переноса НЗП: " + СтрокаСообщения, СтруктураЗаписи, Ложь);
							ВсеОК = Ложь;
						КонецЕсли;
						
						Если ВсеОК Тогда
							НоваяСтрока = ТаблицаДанных.Добавить();
							ЗаполнитьЗначенияСвойств(НоваяСтрока, ВыборкаТаблицы);
							
							Если ЗначениеЗаполнено(ВыборкаТаблицы.ГТД) Тогда
								КоличествоВДокумент = МИН(ВыборкаТаблицы.КоличествоВСтроке, ВыборкаТаблицы.КоличествоГТД, ОстатокНаСкладе);
							Иначе
								КоличествоВДокумент = МИН(ВыборкаТаблицы.Количество, ОстатокНаСкладе);
							КонецЕсли;
							
							СтрокаСумма    = ?(ВыборкаТаблицы.Количество = КоличествоВДокумент, ВыборкаТаблицы.Сумма, (ВыборкаТаблицы.Сумма/ВыборкаТаблицы.Количество)*КоличествоВДокумент);
							СтрокаСумма    = Окр(СтрокаСумма, 2);
							СтрокаСуммаНДС = ?(ВыборкаТаблицы.Количество = КоличествоВДокумент, ВыборкаТаблицы.СуммаНДС, (ВыборкаТаблицы.СуммаНДС/ВыборкаТаблицы.Количество)*КоличествоВДокумент);
							СтрокаСуммаНДС = Окр(СтрокаСуммаНДС, 2);
							СтрокаСуммаУпр = ?(ВыборкаТаблицы.Количество = КоличествоВДокумент, ВыборкаТаблицы.СуммаУпр, (ВыборкаТаблицы.СуммаУпр/ВыборкаТаблицы.Количество)*КоличествоВДокумент);
							СтрокаСуммаУпр = Окр(СтрокаСуммаУпр, 2);
							
							НоваяСтрока.Количество        = КоличествоВДокумент;
							НоваяСтрока.СуммаВсего        = СтрокаСумма;
							НоваяСтрока.СуммаНДС          = СтрокаСуммаНДС;
							НоваяСтрока.СуммаУпр          = СтрокаСуммаУпр;
							
							Если НоваяСтрока.Коэффициент=0 Тогда
								НоваяСтрока.Коэффициент = 1;
							ИначеЕсли НЕ НоваяСтрока.Коэффициент=1 Тогда
								НоваяСтрока.Количество = Окр(НоваяСтрока.Количество/НоваяСтрока.Коэффициент, 3, 1);
							КонецЕсли;
							
							Если ОсвобожденОтНДС Тогда
								НоваяСтрока.СтавкаНДС = СтавкаБезНДС;
								НоваяСтрока.Сумма     = НоваяСтрока.СуммаВсего;
							Иначе
								НоваяСтрока.СтавкаНДС = ВыборкаТаблицы.СтавкаНДС;
								// Рассчитаем значение реквизита "Сумма". Если расчет НДС не ведется, то ЦенаВключаетНДС будет всегда Истина. 
								Если ЦенаВключаетНДС Тогда
									НоваяСтрока.Сумма = НоваяСтрока.СуммаВсего;
								Иначе
									НоваяСтрока.Сумма = НоваяСтрока.СуммаВсего - НоваяСтрока.СуммаНДС;
								КонецЕсли;
							КонецЕсли;
							
							// Пересчитываем цену.
							Если КоличествоВДокумент &lt;&gt; 0 Тогда
								НоваяСтрока.Цена = Окр(НоваяСтрока.Сумма / КоличествоВДокумент,2);
							КонецЕсли;
							
							ОстатокНаСкладе = ОстатокНаСкладе - КоличествоВДокумент;
							ОстатокСумма    = ОстатокСумма    - СтрокаСумма;
							ОстатокСуммаНДС = ОстатокСуммаНДС - СтрокаСуммаНДС;
							ОстатокСуммаУпр = ОстатокСуммаУпр - СтрокаСуммаУпр;
							
							//++gavv заполним связанную таблицу с маркировками
							Если ПустаяСтрока(НоваяСтрока.ИдентификаторТовара) Тогда
								НоваяСтрока.ИдентификаторТовара = Новый УникальныйИдентификатор;
							КонецЕсли;
							Если ЕстьМаркировки Тогда
								СтруктураПоиска = Новый Структура("ЗаказНаряд, Цех, Номенклатура, ХарактеристикаНоменклатуры, ГТД", ТекЗаказНаряд, ТекЦех, НоваяСтрока.Номенклатура, НоваяСтрока.ХарактеристикаНоменклатуры, НоваяСтрока.ГТД);
								СтрокиСМаркировкой = ТаблицаМаркировокВНЗП.НайтиСтроки(СтруктураПоиска);
								Для Каждого ЭлементМассива из СтрокиСМаркировкой Цикл
									НоваяСтрокаМаркировки = ТаблицаДанныхМаркировок.Добавить();
									НоваяСтрокаМаркировки.ИдентификаторТовара = НоваяСтрока.ИдентификаторТовара;
									НоваяСтрокаМаркировки.КодМаркировки = ЭлементМассива.КодМаркировки;
								КонецЦикла;								
							КонецЕсли;  
							//--gavv
							
						КонецЕсли;
						
					КонецЦикла;
				КонецЦикла;
			КонецЦикла;
			
			Если ТаблицаДанных.Количество() &lt;&gt; 0 Тогда
				
				//ТаблицаДанных.Свернуть("Номенклатура, ХарактеристикаНоменклатуры, ЕдиницаИзмерения, Коэффициент, Цена, СтавкаНДС, ГТД", "Количество, Сумма, СуммаНДС, СуммаВсего");
				
				ВходящиеДанные = Новый Структура("Дата, ВалютаДокумента, ДокументОснование, ДоговорВзаиморасчетов, Комментарий, Контрагент, КурсВалютыУпр, КурсДокумента, Организация, ПодразделениеКомпании, Проект, Цех, ТипЦен, ХозОперация, Товары, КодыМаркировки");
				Если ЗначениеЗаполнено(ДатаНачалаЗН) Тогда
					ВходящиеДанные.Дата					= ДатаНачалаЗН;
				Иначе
					ВходящиеДанные.Дата				    = НаДату;
				КонецЕсли;
				
				ИтогСумма    = ТаблицаДанных.Итог("СуммаВсего");
				ИтогСуммаУпр = ТаблицаДанных.Итог("СуммаУпр");
				
				Если ИтогСуммаУпр = 0 Тогда
					КурсВалютыУпрДокумента = КурсВалютыУпр;
				Иначе
					КурсВалютыУпрДокумента = ИтогСумма / ИтогСуммаУпр;
				КонецЕсли;
				
				ВходящиеДанные.ВалютаДокумента			= ВалютаРегл;
				ВходящиеДанные.ДокументОснование		= ТекЗаказНаряд;
				ВходящиеДанные.ДоговорВзаиморасчетов	= ТекДоговорВзаиморасчетов;
				ВходящиеДанные.Контрагент				= ТекКонтрагент;
				
				ВходящиеДанные.КурсВалютыУпр			= КурсВалютыУпрДокумента;
				ВходящиеДанные.КурсДокумента			= КурсДокумента;
				ВходящиеДанные.Организация				= ТекОрганизация;
				ВходящиеДанные.ПодразделениеКомпании	= ТекПодразделение;
				ВходящиеДанные.Цех						= ТекЦех;
				//ВходящиеДанные.СтатьяОприходованияТМЦ	= Параметры.СтатьяОприходованияТМЦ;
				
				ВходящиеДанные.ТипЦен					= ТипЦенЗН;
				ВходящиеДанные.ХозОперация				= ТекХозОперация;
				
				ВходящиеДанные.Комментарий				= "Перенос остатков незавершенного производства из Альфа-Авто 5. Заказ-наряд-источник: " + Строка(ТекЗаказНаряд)+", партия-источник: "+ Строка(ТекПартия);
				ВходящиеДанные.Товары					= ТаблицаДанных;
				ВходящиеДанные.КодыМаркировки			= ТаблицаДанныхМаркировок;                                                         
				
				Если обЗначениеНеЗаполнено(ВходящиеДанные.ДокументОснование) Тогда
					
					//++gavv - у остатка НЗП не обнаружился заказ-наряд (сохранил логирование, сам такую проблему не видел)
					СтрокаСообщения = "При выгрузке остатков НЗП по партии: " + Строка(ТекПартия) + " обнаружен незаполненный документ-основание (заказ-наряд). Остаток НЗП перенесен не будет!";
					СтруктураЗаписи = Новый Структура;
					СтруктураЗаписи.Вставить("ЗаказНаряд", Строка(ТекЗаказНаряд));
					ОбщиеПроцедурыФункции.ЗаписатьВПротоколВыполнения("Проблема переноса НЗП: " + СтрокаСообщения, СтруктураЗаписи, Ложь);
					
				ИначеЕсли ВходящиеДанные.ДокументОснование.Состояние = Справочники.ВидыСостоянийЗаказНарядов.Закрыт Тогда
					
					//++gavv - у остатка НЗП обнаружился закрытый заказ-наряд (сохранил логирование, сам такую проблему не видел)
					СтрокаСообщения = "При выгрузке остатков НЗП по партии: " + Строка(ТекПартия) + " обнаружен закрытый заказ-наряд "+ Строка(ТекЗаказНаряд) + ". Остаток НЗП перенесен не будет!";
					СтруктураЗаписи = Новый Структура;
					СтруктураЗаписи.Вставить("ЗаказНаряд", Строка(ТекЗаказНаряд));
					ОбщиеПроцедурыФункции.ЗаписатьВПротоколВыполнения("Проблема переноса НЗП: " + СтрокаСообщения, СтруктураЗаписи, Ложь);
				Иначе
					//***
					ВыгрузитьПоПравилу(,, ВходящиеДанные,, "ОстаткиТоваровВПроизводстве");
				КонецЕсли;
				
				ТаблицаДанных.Очистить();
			КонецЕсли;
			
		КонецЦикла;
	КонецЦикла;
КонецЦикла;</ПередОбработкойПравила>
