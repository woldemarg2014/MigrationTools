		ВходящиеДанные.Вставить("ДоговорВзаиморасчетов", ВыборкаЗаказов.ДоговорВзаиморасчетов);
		ВходящиеДанные.Вставить("Карточка"             , ВыборкаЗаказов.Карточка);
		ВходящиеДанные.Вставить("Контрагент"           , ВыборкаЗаказов.Контрагент);
		ВходящиеДанные.Комментарий           = "Перенос заказов товара клиентов из Альфа-Авто 5. Заказ-источник: " + Строка(ТекЗаказ);
		ВходящиеДанные.КурсВалютыУпр         = КурсВалютыУпрДокумента;
		ВходящиеДанные.КурсДокумента         = КурсДокумента;
		ВходящиеДанные.Менеджер              = ВыборкаЗаказов.Менеджер;
		ВходящиеДанные.Вставить("Организация"          , ТекОрганизация);
		ВходящиеДанные.Вставить("ПодразделениеКомпании", ТекПодразделение);
