			<Правило Отключить="false">
				<Код>Остатки_Взаиморасчеты</Код>
				<Наименование>Остатки_Взаиморасчеты</Наименование>
				<Порядок>350</Порядок>
				<КодПравилаКонвертации>ОстаткиВзаиморасчетов                             </КодПравилаКонвертации>
				<СпособОтбораДанных>ПроизвольныйАлгоритм</СпособОтбораДанных>
				<ПередОбработкойПравила>////*** Выгрузка остатков взаиморасчетов
НаДату	= ?(ЗначениеЗаполнено(Параметры.ДатаВыгрузкиОстатков), Параметры.ДатаВыгрузкиОстатков, ТекущаяДата());

Запрос = Новый Запрос();
Запрос.УстановитьПараметр("ДатаСведений", КонецДня(НаДату));

Запрос.Текст =
"ВЫБРАТЬ
|	ВзаиморасчетыКомпанииОстатки.Контрагент,
|	ВзаиморасчетыКомпанииОстатки.ДоговорВзаиморасчетов,
|	ВзаиморасчетыКомпанииОстатки.ДоговорВзаиморасчетов.Организация КАК Организация,
|	ВзаиморасчетыКомпанииОстатки.ДоговорВзаиморасчетов.Подразделение КАК Подразделение,
|	ВзаиморасчетыКомпанииОстатки.ДоговорВзаиморасчетов.ВалютаВзаиморасчетов КАК ВалютаВзаиморасчетов,
|	СУММА(ВзаиморасчетыКомпанииОстатки.СуммаОстаток) КАК СуммаОстаток,
|	СУММА(ВзаиморасчетыКомпанииОстатки.СуммаУпрОстаток) КАК СуммаУпрОстаток
|ПОМЕСТИТЬ ВТ_Остатки
|ИЗ
|	РегистрНакопления.ВзаиморасчетыКомпании.Остатки(&amp;ДатаСведений, ) КАК ВзаиморасчетыКомпанииОстатки
|
|СГРУППИРОВАТЬ ПО
|	ВзаиморасчетыКомпанииОстатки.Контрагент,
|	ВзаиморасчетыКомпанииОстатки.ДоговорВзаиморасчетов,
|	ВзаиморасчетыКомпанииОстатки.ДоговорВзаиморасчетов.Организация,
|	ВзаиморасчетыКомпанииОстатки.ДоговорВзаиморасчетов.Подразделение,
|	ВзаиморасчетыКомпанииОстатки.ДоговорВзаиморасчетов.ВалютаВзаиморасчетов
|
|ИМЕЮЩИЕ
|	СУММА(ВзаиморасчетыКомпанииОстатки.СуммаОстаток) &lt;&gt; 0
|;
|
|////////////////////////////////////////////////////////////////////////////////
|ВЫБРАТЬ
|	ВТ_Остатки.Контрагент,
|	ВТ_Остатки.ДоговорВзаиморасчетов,
|	ВТ_Остатки.Организация КАК Организация,
|	ВТ_Остатки.Подразделение КАК Подразделение,
|	ВТ_Остатки.ВалютаВзаиморасчетов КАК ВалютаВзаиморасчетов,
|	ВЫБОР
|		КОГДА ВТ_Остатки.СуммаОстаток &gt; 0
|			ТОГДА ВТ_Остатки.СуммаОстаток
|		ИНАЧЕ 0
|	КОНЕЦ КАК СуммаДебет,
|	ВЫБОР
|		КОГДА ВТ_Остатки.СуммаОстаток &lt; 0
|			ТОГДА -ВТ_Остатки.СуммаОстаток
|		ИНАЧЕ 0
|	КОНЕЦ КАК СуммаКредит,
|	ВЫБОР
|		КОГДА ВТ_Остатки.СуммаУпрОстаток &gt; 0
|			ТОГДА ВТ_Остатки.СуммаУпрОстаток
|		ИНАЧЕ 0
|	КОНЕЦ КАК СуммаДебетУпр,
|	ВЫБОР
|		КОГДА ВТ_Остатки.СуммаУпрОстаток &lt; 0
|			ТОГДА -ВТ_Остатки.СуммаУпрОстаток
|		ИНАЧЕ 0
|	КОНЕЦ КАК СуммаКредитУпр
|ИЗ
|	ВТ_Остатки КАК ВТ_Остатки
|ИТОГИ ПО
|	Организация,
|	Подразделение,
|	ВалютаВзаиморасчетов";

ТаблицаДанных = Новый ТаблицаЗначений();
ТаблицаДанных.Колонки.Добавить("Контрагент");
ТаблицаДанных.Колонки.Добавить("ДоговорВзаиморасчетов");
ТаблицаДанных.Колонки.Добавить("КурсВалютыВзаиморасчетов");
ТаблицаДанных.Колонки.Добавить("СуммаДебет");
ТаблицаДанных.Колонки.Добавить("СуммаКредит");
ТаблицаДанных.Колонки.Добавить("СуммаДебетУпр");
ТаблицаДанных.Колонки.Добавить("СуммаКредитУпр");

ВалютаУпр		= Константы.ВалютаУправленческогоУчетаКомпании.Получить();
СтруктураКурса	= обКурсДляВалюты(ВалютаУпр, НаДату);
КурсВалютыУпр	= СтруктураКурса.Курс / ?(СтруктураКурса.Кратность = 0, 1, СтруктураКурса.Кратность);

ВыборкаОрганизация = Запрос.Выполнить().Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
Пока ВыборкаОрганизация.Следующий() Цикл
	ТекОрганизация = ВыборкаОрганизация.Организация;
	ВыборкаПодразделение = ВыборкаОрганизация.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	Пока ВыборкаПодразделение.Следующий() Цикл
		
		ТекПодразделение = ВыборкаПодразделение.Подразделение;
		ВыборкаВалюта = ВыборкаПодразделение.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
		Пока ВыборкаВалюта.Следующий() Цикл
			ТекВалютаВзаиморасчетов = ВыборкаВалюта.ВалютаВзаиморасчетов;
			
			СтруктураКурса = обКурсДляВалюты(ТекВалютаВзаиморасчетов, НаДату);
			КурсДокумента = СтруктураКурса.Курс / ?(СтруктураКурса.Кратность = 0, 1, СтруктураКурса.Кратность);
			
			Выборка = ВыборкаВалюта.Выбрать(ОбходРезультатаЗапроса.Прямой);
			Пока Выборка.Следующий() Цикл
				
				НоваяСтрока = ТаблицаДанных.Добавить();
				ЗаполнитьЗначенияСвойств(НоваяСтрока, Выборка);
				НоваяСтрока.КурсВалютыВзаиморасчетов = КурсДокумента;
				
			КонецЦикла;
			Если ТаблицаДанных.Количество() &lt;&gt; 0 Тогда
				
				ИтогСуммаДебет    = ТаблицаДанных.Итог("СуммаДебет");
				ИтогСуммаДебетУпр = ТаблицаДанных.Итог("СуммаДебетУпр");
				
				ИтогСуммаКредит    = ТаблицаДанных.Итог("СуммаКредит");
				ИтогСуммаКредитУпр = ТаблицаДанных.Итог("СуммаКредитУпр");
				
				Если НЕ ИтогСуммаДебетУпр = 0 Тогда
					КурсВалютыУпрДокумента = ИтогСуммаДебет / ИтогСуммаДебетУпр;
				ИначеЕсли НЕ ИтогСуммаКредитУпр = 0 Тогда
					КурсВалютыУпрДокумента = ИтогСуммаКредит / ИтогСуммаКредитУпр;
				Иначе
					КурсВалютыУпрДокумента = КурсВалютыУпр;
				КонецЕсли;
				
				ВходящиеДанные = Новый Структура("Дата, ВалютаДокумента, Комментарий, КурсВалютыУпр, КурсДокумента, Организация, ПодразделениеКомпании, СуммаДебет, СуммаДокумента, СуммаКредит, ХозОперация, Состав");				
				ВходящиеДанные.Дата						= НаДату;
				ВходящиеДанные.ВалютаДокумента			= ТекВалютаВзаиморасчетов;
				ВходящиеДанные.КурсВалютыУпр			= КурсВалютыУпрДокумента;
				ВходящиеДанные.КурсДокумента			= КурсДокумента;
				ВходящиеДанные.Организация				= ТекОрганизация;
				ВходящиеДанные.ПодразделениеКомпании	= ТекПодразделение;
				ВходящиеДанные.СуммаДебет				= ИтогСуммаДебет;
				ВходящиеДанные.СуммаКредит				= ИтогСуммаКредит;
				ВходящиеДанные.СуммаДокумента			= ВходящиеДанные.СуммаДебет + ВходящиеДанные.СуммаКредит;
				ВходящиеДанные.ХозОперация				= Справочники.ХозОперации.ВводОстатковВзаиморасчетов;
				
				ВходящиеДанные.Комментарий				= "Перенос остатков взаиморасчетов из Альфа-Авто 5";
				ВходящиеДанные.Состав					= ТаблицаДанных;
				
				//***
				ВыгрузитьПоПравилу(,, ВходящиеДанные,, "ОстаткиВзаиморасчетов");
				ТаблицаДанных.Очистить();
			КонецЕсли;
		КонецЦикла;
	КонецЦикла;
КонецЦикла;</ПередОбработкойПравила>
