		КонецЕсли; 
		
		//если не заполнен артикул для поиска - заполняем его
		Если ПустаяСтрока(Объект.АртикулДляПоиска) Тогда
			Объект.АртикулДляПоиска = ОбщегоНазначенияАвтосалонКлиентСервер.ВАртикулДляПоиска(Объект.Артикул);
			
			СтрокаСообщения = СтрокаСообщения + ?(ПустаяСтрока(СтрокаСообщения),"","; ") + "заполнен реквизит 'АртикулДляПоиска'";
		КонецЕсли;
		
		//если артикулы не заполнены - ставим режим обмена данными
		Если ПустаяСтрока(Объект.Артикул)
			ИЛИ ПустаяСтрока(Объект.АртикулДляПоиска)
			ИЛИ ПустаяСтрока(Объект.Наименование) Тогда
			Объект.ОбменДанными.Загрузка = Истина;
			
			СтрокаСообщения = СтрокаСообщения + ?(ПустаяСтрока(СтрокаСообщения),"",". ") + "В номенклатуре-источнике не были заполнены реквизиты 'Артикул' или 'АртикулДляПоиска' или 'Наименование'. Загружаем в режиме обмена.";
		КонецЕсли;
	КонецЕсли;
	
	Если Не ПустаяСтрока(СтрокаСообщения) Тогда
		СтруктураЗаписи = Новый Структура;
		СтруктураЗаписи.Вставить("Номенклатура_Код", Объект.Код);
		СтруктураЗаписи.Вставить("Номенклатура_Наименование", Объект.Наименование);
		ОбщиеПроцедурыФункции.ЗаписатьВПротоколВыполнения("Отрегулирована проблема в справочнике 'Номенклатура' базы-приемника. " + СтрокаСообщения, СтруктураЗаписи, Ложь);
	КонецЕсли;	
	
КонецЕсли;
