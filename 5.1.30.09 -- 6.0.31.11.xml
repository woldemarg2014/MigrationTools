				СтавкаНДС = НоваяСтрока.СтавкаНДС.Ставка;
				НоваяСтрока.СуммаНДС  = Окр((НоваяСтрока.СуммаВсего * СтавкаНДС)/(100 + СтавкаНДС), 2);
				
