|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.Разукомплектация КАК Разукомплектация
|		ПО ТоварыПартий.Партия = Разукомплектация.Ссылка
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ПоступлениеТоваров КАК ПоступлениеТоваров
|		ПО ТоварыПартий.Партия = ПоступлениеТоваров.Ссылка
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.АвансовыйОтчет КАК АвансовыйОтчет
|		ПО ТоварыПартий.Партия = АвансовыйОтчет.Ссылка
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ВводОстатковТоваров КАК ВводОстатковТоваров
|		ПО ТоварыПартий.Партия = ВводОстатковТоваров.Ссылка
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ВводОстатковТоваровВПроизводстве КАК ВводОстатковТоваровВПроизводстве
|		ПО ТоварыПартий.Партия = ВводОстатковТоваровВПроизводстве.Ссылка
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.Инвентаризация КАК Инвентаризация
|		ПО ТоварыПартий.Партия = Инвентаризация.Ссылка
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ПеремещениеТоваров КАК ПеремещениеТоваров
|		ПО ТоварыПартий.Партия = ПеремещениеТоваров.Ссылка
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ПересортицаТоваров КАК ПересортицаТоваров
|		ПО ТоварыПартий.Партия = ПересортицаТоваров.Ссылка
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ПоступлениеТоваров.Товары КАК ПоступлениеТоваровТовары
|		ПО ТоварыПартий.Партия = ПоступлениеТоваровТовары.Ссылка
|			И ТоварыПартий.Номенклатура = ПоступлениеТоваровТовары.Номенклатура
|			И ТоварыПартий.ХарактеристикаНоменклатуры = ПоступлениеТоваровТовары.ХарактеристикаНоменклатуры
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.АвансовыйОтчет.Товары КАК АвансовыйОтчетТовары
|		ПО ТоварыПартий.Партия = АвансовыйОтчетТовары.Ссылка
|			И ТоварыПартий.Номенклатура = АвансовыйОтчетТовары.Номенклатура
|			И ТоварыПартий.ХарактеристикаНоменклатуры = АвансовыйОтчетТовары.ХарактеристикаНоменклатуры
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ВводОстатковТоваров.Товары КАК ВводОстатковТоваровТовары
|		ПО ТоварыПартий.Партия = ВводОстатковТоваровТовары.Ссылка
|			И ТоварыПартий.Номенклатура = ВводОстатковТоваровТовары.Номенклатура
|			И ТоварыПартий.ХарактеристикаНоменклатуры = ВводОстатковТоваровТовары.ХарактеристикаНоменклатуры
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ВводОстатковТоваровВПроизводстве.Товары КАК ВводОстатковТоваровВПроизводствеТовары
|		ПО ТоварыПартий.Партия = ВводОстатковТоваровВПроизводствеТовары.Ссылка
|			И ТоварыПартий.Номенклатура = ВводОстатковТоваровВПроизводствеТовары.Номенклатура
|			И ТоварыПартий.ХарактеристикаНоменклатуры = ВводОстатковТоваровВПроизводствеТовары.ХарактеристикаНоменклатуры
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.Инвентаризация.Товары КАК ИнвентаризацияТовары
|		ПО ТоварыПартий.Партия = ИнвентаризацияТовары.Ссылка
|			И ТоварыПартий.Номенклатура = ИнвентаризацияТовары.Номенклатура
|			И ТоварыПартий.ХарактеристикаНоменклатуры = ИнвентаризацияТовары.ХарактеристикаНоменклатуры
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ПеремещениеТоваров.Товары КАК ПеремещениеТоваровТовары
|		ПО ТоварыПартий.Партия = ПеремещениеТоваровТовары.Ссылка
|			И ТоварыПартий.Номенклатура = ПеремещениеТоваровТовары.Номенклатура
|			И ТоварыПартий.ХарактеристикаНоменклатуры = ПеремещениеТоваровТовары.ХарактеристикаНоменклатуры
|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ПересортицаТоваров.Товары КАК ПересортицаТоваровТовары
|		ПО ТоварыПартий.Партия = ПересортицаТоваровТовары.Ссылка
|			И ТоварыПартий.Номенклатура = ПересортицаТоваровТовары.Номенклатура
|			И ТоварыПартий.ХарактеристикаНоменклатуры = ПересортицаТоваровТовары.ХарактеристикаНоменклатуры
|
|УПОРЯДОЧИТЬ ПО
|	НомерСтроки УБЫВ
|ИТОГИ
|	МАКСИМУМ(ТипЦен),
|	МАКСИМУМ(Контрагент),
|	МАКСИМУМ(ДоговорВзаиморасчетов),
|	МАКСИМУМ(Количество),
|	МАКСИМУМ(Сумма),
|	МАКСИМУМ(СуммаНДС),
|	МАКСИМУМ(СуммаУпр),
|	МАКСИМУМ(НомерСтроки)
|ПО
|	СкладКомпании,
|	Партия,
|	Номенклатура,
|	ХарактеристикаНоменклатуры";

ХозОперацияКомиссия = Справочники.ХозОперации.ВводОстатковТоваровПринятыхНаКомиссию;
ХозОперацияОстатки  = Справочники.ХозОперации.ВводОстатковТоваров;

СтавкаБезНДС = Справочники.СтавкиНДС.БезНДС;
Для Каждого ВидПартий Из МассивВидовПартий Цикл
	Если ВидПартий = Перечисления.СтатусыПартий.ТоварКупленный Тогда
		// Остатки купленного товара
		Запрос.Текст = СтрЗаменить(ТекстЗапроса, "&amp;УСЛОВИЕ_НА_СТАТУС_ПАРТИИ", "Остатки.СтатусПартии &lt;&gt; &amp;СтатусПартии");
	ИначеЕсли ВидПартий=Перечисления.СтатусыПартий.ТоварПринятыйКомиссия Тогда
		// Остатки товаров принятых на комиссию
		Запрос.Текст = СтрЗаменить(ТекстЗапроса, "&amp;УСЛОВИЕ_НА_СТАТУС_ПАРТИИ", "Остатки.СтатусПартии = &amp;СтатусПартии");
	Иначе
		Продолжить;
	КонецЕсли;
	
	ТаблицаГТД.Очистить();
	ТаблицаДанных.Очистить();
	
	ВыборкаСклады = Запрос.Выполнить().Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	Пока ВыборкаСклады.Следующий() Цикл
		
		ТекСкладКомпании = ВыборкаСклады.СкладКомпании;
		
		СтратегияСписанияПартий = ТекСкладКомпании.СтратегияСписанияПартийТоваровПоДатам;
		Если НЕ ЗначениеЗаполнено(СтратегияСписанияПартий) Тогда
			СтратегияСписанияПартий = ОсновнаяСтратегияСписанияПартий;
		КонецЕсли;
		
		СписаниеПоСредней = (СтратегияСписанияПартий = Перечисления.СтратегияСписанияПартийТоваровПоДатам.Средняя);
		
		ТекПодразделение = ТекСкладКомпании.Подразделение;
		ТекОрганизация   = ТекПодразделение.Организация;
		Розничный        = ТекСкладКомпании.Розничный;
		
		Если (ЗначениеЗаполнено(ТекПодразделение) И ТекПодразделение.ОсвобожденОтНДС) ИЛИ
			(ЗначениеЗаполнено(ТекОрганизация) И ТекОрганизация.ОсвобожденОтНДС) Тогда
			ОсвобожденОтНДС = Истина;
		Иначе
			ОсвобожденОтНДС = Ложь;
		КонецЕсли;
		
		ВыборкаПартии = ВыборкаСклады.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
		Пока ВыборкаПартии.Следующий() Цикл
			ТекПартия = ВыборкаПартии.Партия;
			Если обЗначениеНеЗаполнено(ТекПартия) Тогда
				Продолжить;
			КонецЕсли;
			
			ТекТипЦен                = ВыборкаПартии.ТипЦен;
			ТекКонтрагент            = ВыборкаПартии.Контрагент;
			ТекДоговорВзаиморасчетов = ВыборкаПартии.ДоговорВзаиморасчетов;
			
			ЦенаВключаетНДС          = (ТекТипЦен.Пустая() ИЛИ ТекТипЦен.ЦенаВключаетНДС);
			
			НомерЗаписи    = 1;
			ВыборкаНоменклатура = ВыборкаПартии.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
			Пока ВыборкаНоменклатура.Следующий() Цикл
				
				Если ВыборкаНоменклатура.ЕдиницаИзмерения = ПустаяЕдиница Тогда
					Продолжить;
				КонецЕсли;
				
				ВыборкаХарактеристик = ВыборкаНоменклатура.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
				Пока ВыборкаХарактеристик.Следующий() Цикл
					
					//ОстатокНаСкладе = ВыборкаХарактеристик.КоличествоГТД;
					ОстатокНаСкладе = ВыборкаХарактеристик.Количество;
					ВыборкаТаблицы  = ВыборкаХарактеристик.Выбрать(ОбходРезультатаЗапроса.Прямой);
					
					ОстатокСумма    = ВыборкаХарактеристик.Сумма;
					ОстатокСуммаНДС = ВыборкаХарактеристик.СуммаНДС;
					ОстатокСуммаУпр = ВыборкаХарактеристик.СуммаУпр;
					
					Пока ВыборкаТаблицы.Следующий() Цикл
						
						Если ОстатокНаСкладе &lt;=0 Тогда
							Прервать;
						КонецЕсли;
						
						НоваяСтрока = ТаблицаДанных.Добавить();
						ЗаполнитьЗначенияСвойств(НоваяСтрока, ВыборкаТаблицы);
						
						
						//Если СтрокаВыборки.ГТД&lt;&gt;NULL И СтрокаВыборки.КоличествоГТД&lt;=0 Тогда
						//	Продолжить;
						//КонецЕсли;
						Если ЗначениеЗаполнено(ВыборкаТаблицы.ГТД) Тогда
							КоличествоВДокумент = МИН(ВыборкаТаблицы.КоличествоВСтроке, ВыборкаТаблицы.КоличествоГТД, ОстатокНаСкладе);
						Иначе
							КоличествоВДокумент = МИН(ВыборкаТаблицы.Количество, ОстатокНаСкладе);
						КонецЕсли;
						
						СтрокаСумма    = ?(ВыборкаТаблицы.Количество = КоличествоВДокумент, ВыборкаТаблицы.Сумма, (ВыборкаТаблицы.Сумма/ВыборкаТаблицы.Количество)*КоличествоВДокумент);
						СтрокаСумма    = Окр(СтрокаСумма, 2);
						СтрокаСуммаНДС = ?(ВыборкаТаблицы.Количество = КоличествоВДокумент, ВыборкаТаблицы.СуммаНДС, (ВыборкаТаблицы.СуммаНДС/ВыборкаТаблицы.Количество)*КоличествоВДокумент);
						СтрокаСуммаНДС = Окр(СтрокаСуммаНДС, 2);
						СтрокаСуммаУпр = ?(ВыборкаТаблицы.Количество = КоличествоВДокумент, ВыборкаТаблицы.СуммаУпр, (ВыборкаТаблицы.СуммаУпр/ВыборкаТаблицы.Количество)*КоличествоВДокумент);
						СтрокаСуммаУпр = Окр(СтрокаСуммаУпр, 2);
						
						НоваяСтрока.Количество        = КоличествоВДокумент;
						//НоваяСтрока.КоличествоБазовое = КоличествоВДокумент;
						НоваяСтрока.СуммаВсего        = СтрокаСумма;
						НоваяСтрока.СуммаНДС          = СтрокаСуммаНДС;
						НоваяСтрока.СуммаУпр          = СтрокаСуммаУпр;
						
						Если НоваяСтрока.Коэффициент=0 Тогда
							НоваяСтрока.Коэффициент = 1;
						ИначеЕсли НЕ НоваяСтрока.Коэффициент=1 Тогда
							НоваяСтрока.Количество = Окр(НоваяСтрока.Количество/НоваяСтрока.Коэффициент, 3, 1);
						КонецЕсли;
						
						Если ОсвобожденОтНДС Тогда
							НоваяСтрока.СтавкаНДС = СтавкаБезНДС;
							НоваяСтрока.Сумма     = НоваяСтрока.СуммаВсего;
						Иначе
							НоваяСтрока.СтавкаНДС = ВыборкаТаблицы.СтавкаНДС;
							// Рассчитаем значение реквизита "Сумма". Если расчет НДС не ведется, то ЦенаВключаетНДС будет всегда Истина. 
							Если ЦенаВключаетНДС Тогда
								НоваяСтрока.Сумма = НоваяСтрока.СуммаВсего;
							Иначе
								НоваяСтрока.Сумма = НоваяСтрока.СуммаВсего - НоваяСтрока.СуммаНДС;
							КонецЕсли;
							
						КонецЕсли;
						
						// Пересчитываем цену.
						Если КоличествоВДокумент &lt;&gt; 0 Тогда
							НоваяСтрока.Цена = Окр(НоваяСтрока.Сумма / КоличествоВДокумент,2);
						КонецЕсли;
						
						Если Розничный Тогда
							НоваяСтрока.ЦенаРозничная  = НоваяСтрока.Цена;
							НоваяСтрока.СуммаРозничная = НоваяСтрока.Сумма;
						КонецЕсли;
						
						ОстатокНаСкладе = ОстатокНаСкладе - КоличествоВДокумент;
						ОстатокСумма    = ОстатокСумма    - СтрокаСумма;
						ОстатокСуммаНДС = ОстатокСуммаНДС - СтрокаСуммаНДС;
						ОстатокСуммаУпр = ОстатокСуммаУпр - СтрокаСуммаУпр;
						
						// Предусмотреть сохранение ГТД при учете по среднему. ГТД берем из последнего прихода по товару
						Если обЗначениеНеЗаполнено(НоваяСтрока.ГТД) И СписаниеПоСредней Тогда
							
							
						ЗапросГТД = Запросы.ЗапросГТД;
						ЗапросГТД.УстановитьПараметр("Номенклатура", НоваяСтрока.Номенклатура);
						ЗапросГТД.УстановитьПараметр("ХарактеристикаНоменклатуры", НоваяСтрока.ХарактеристикаНоменклатуры);
							
							РезультатЗапросаГТД = ЗапросГТД.Выполнить();
							Если НЕ РезультатЗапросаГТД.Пустой() Тогда
								ВыборкаГТД = РезультатЗапросаГТД.Выбрать();
								Если ВыборкаГТД.Следующий() Тогда
									НоваяСтрока.ГТД = ВыборкаГТД.ГТД;
								КонецЕсли;
							КонецЕсли;
							
						КонецЕсли;
						
						НомерЗаписи = НомерЗаписи + 1;
						
						Если (НомерЗаписи&gt;10000) Тогда
							
							НомерЗаписи = 1;
							Если ТаблицаДанных.Количество() &lt;&gt; 0 Тогда
								
								ИтогСумма    = ТаблицаДанных.Итог("СуммаВсего");
								ИтогСуммаУпр = ТаблицаДанных.Итог("СуммаУпр");
								
								Если ИтогСуммаУпр = 0 Тогда
									КурсВалютыУпрДокумента = КурсВалютыУпр;
								Иначе
									КурсВалютыУпрДокумента = ИтогСумма / ИтогСуммаУпр;
								КонецЕсли;
								
								ВходящиеДанные	= Новый Структура("Дата, Автор, ВалютаДокумента, ДатаОперации, ДатаСоздания, ДоговорВзаиморасчетов, Комментарий, Контрагент, КурсВалютыУпр, КурсДокумента, Организация, ПодразделениеКомпании, Проект, РегламентированныйУчет, СкладКомпании, СтатьяОприходованияТМЦ, СуммаДокумента, ТипЦен, Идентификатор, ХозОперация, Товары");				
								ВходящиеДанные.Дата						= НаДату;
								ВходящиеДанные.Автор					= ПараметрыСеанса.Пользователь;
								ВходящиеДанные.ВалютаДокумента			= ВалютаРегл;
								ВходящиеДанные.ДатаОперации				= НаДату;
								ВходящиеДанные.ДатаСоздания				= НаДату;
								ВходящиеДанные.ДоговорВзаиморасчетов	= ТекДоговорВзаиморасчетов;
								ВходящиеДанные.Контрагент				= ТекКонтрагент;
								ВходящиеДанные.КурсВалютыУпр			= КурсВалютыУпрДокумента;
								ВходящиеДанные.КурсДокумента			= КурсДокумента;
								ВходящиеДанные.Организация				= ТекОрганизация;
								ВходящиеДанные.ПодразделениеКомпании	= ТекПодразделение;
								//ВходящиеДанные.Проект					= Параметры.ПроектВводаОстатков;
								ВходящиеДанные.РегламентированныйУчет	= Ложь;
								ВходящиеДанные.СкладКомпании			= ТекСкладКомпании;
								//ВходящиеДанные.СтатьяОприходованияТМЦ	= Параметры.СтатьяОприходованияТМЦ;
								ВходящиеДанные.СуммаДокумента			= ИтогСумма;
								ВходящиеДанные.ТипЦен					= ТекТипЦен;
								ВходящиеДанные.Идентификатор			= Строка(ТекПартия.УникальныйИдентификатор());
								
								Если ВидПартий = Перечисления.СтатусыПартий.ТоварПринятыйКомиссия Тогда
									ВходящиеДанные.ХозОперация			= ХозОперацияКомиссия;
								Иначе					
									ВходящиеДанные.ХозОперация			= ХозОперацияОстатки;
